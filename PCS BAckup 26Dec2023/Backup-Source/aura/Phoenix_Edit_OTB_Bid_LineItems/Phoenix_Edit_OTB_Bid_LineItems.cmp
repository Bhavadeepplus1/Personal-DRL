            <aura:if isTrue="{!and(and(v.BidTypeVal!='Price Change',v.BidTypeVal!='Sales Out Promotional Discount'),and(v.BidTypeVal!='Sales Out Rebate',v.BidTypeVal!='Customer Rebate Change'))}">
                <lightning:tab label="SCM View" id="SCM" onactive="{!c.initSCMCmp}">
                    <c:SCMView aura:id="SCMChildCmp" title="{!(v.bidNumber+'-'+v.bidName)}" recordId='{!v.recordId}'/>
                </lightning:tab>
            </aura:if>
        </aura:if>
        <aura:if isTrue="{!v.BidTypeVal!='Volume Review Only'}">
            <lightning:tab label="Finance View" id="FIV" onactive="{!c.initFinnaceCmp}"> 
                <c:Phoenix_FinanceView	aura:id="financeChildCmp" title="{!(v.bidNumber+'-'+v.bidName)}" recordId='{!v.recordId}'/>
            </lightning:tab>
        </aura:if>
        <aura:if isTrue="{!v.BidTypeVal!='Volume Review Only'}">
            <lightning:tab label="NDC Finance View" onactive="{!c.initNDCFinanceCmp}"> 
                <c:NDCFinanceView aura:id="NDCFinance" title="{!(v.bidNumber+'-'+v.bidName)}" recordId='{!v.recordId}'/>
            </lightning:tab>
        </aura:if>
        <aura:if isTrue="{!v.BidTypeVal!='Short Dated OTB'}">
            <lightning:tab label="Management Approval"> 
                <c:Phoenix_FinanceApprovalGrid aura:id="ApprovalGrid" title="{!(v.bidNumber+'-'+v.bidName)}" recordId='{!v.recordId}'/>
            </lightning:tab>
        </aura:if>
        
        <lightning:tab label="Contracts View" id="Contracts" onactive="{!c.initContractsView}">
            <c:OTBContractsView aura:id="ContractsTab"  recordId='{!v.recordId}'/>
        </lightning:tab>
        <lightning:tab label="Rejected View" id="RejectedStatus" onactive="{!c.initRejectedStatus}">
            <c:OTBRejectedLineItemsTab aura:id="RejectedStatusChildCmp" recordId='{!v.recordId}' otbDirIndirect="{!v.otbDirIndirect}"/>
        </lightning:tab>
        <lightning:tab label="Bid Summary View" id="CustomerResponseNew" onactive="{!c.initNewCustomerView}">
            <c:CustomerResponseLinesView aura:id="CustomersTabNew"  title="{!(v.bidNumber+'-'+v.bidName)}" recordId='{!v.recordId}'/>
        </lightning:tab>
    </lightning:tabset>
</aura:component>