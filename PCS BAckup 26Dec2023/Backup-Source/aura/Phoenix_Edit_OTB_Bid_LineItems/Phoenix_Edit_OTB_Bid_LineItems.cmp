                    </aura:if>
                    <!-- <lightning:button label="Submit For Approval" onclick="{!c.submitFor}"/>-->
                    <!--
                    <aura:if isTrue="{!and(and(and(v.BidAprrovalStatus!='Draft',v.BidAprrovalStatus==equals(v.BidTypeVal,'Good Dated OTB')?'Marketing':'SCM &amp; Marketing'),v.BidLineItemListAll.length > 0),and(and(v.isMarketingApprovePerson==v.loggedInUserName,v.hasDirectorLines==true),and(v.showProceedBtn==true,v.showSaveCancelBtn==false)))}">
                    <lightning:button variant="brand" label="Submit" title="Proceed" onclick="{! c.saveToProceed }"/>
                       
                    </aura:if>
-->
                    
                    <aura:if isTrue="{!and(v.BidAprrovalStatus!='Draft',and((v.BidAprrovalStatus==(or(or(equals(v.BidTypeVal,'New Product Launch'),equals(v.BidTypeVal,'Customer Rebate Change')),or(equals(v.BidTypeVal,'Price Change'),equals(v.BidTypeVal,'One Time Buy Good Dated Shelf Life for New Product Launch')))?'Marketing':'SCM &amp; Marketing')), and(v.BidLineItemListAll.length > 0,and(or(v.deligatedUserName==v.loggedInUserName,v.isMarketingApprovePerson==v.loggedInUserName),and(v.showProceedBtn==true,and(v.showSaveCancelBtn==false,v.hasDirectorLines))))))}">
                        <lightning:button variant="brand" label="Submit" title="Proceed" onclick="{! c.saveToProceed }"/>
                        <!--<aura:set attribute="else">
                            {!(v.isMarketingApprovePerson==v.loggedInUserName)}
                        {!v.hasDirectorLines}
                            {!v.showProceedBtn}
                            {!v.showSaveCancelBtn}
                            {!v.BidAprrovalStatus}
                            {!v.v.BidLineItemListAll.length}
                        </aura:set>-->
                    </aura:if>
                    
                    <!--
                    <aura:if isTrue="{!and(v.isMarketingApprovePerson==v.loggedInUserName,and(v.BidAprrovalStatus=='Marketing',v.BidLineItemListAll.length > 0),and(and(v.isMarketingApprovePerson==v.loggedInUserName,v.hasDirectorLines==true),and(v.showProceedBtn==true,v.showSaveCancelBtn==false)))}">
                    <lightning:button variant="brand" label="Submit" title="Proceed" onclick="{! c.saveToProceed }"/>
                    
                    </aura:if>
-->
                    
