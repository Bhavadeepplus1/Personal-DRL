                                    <aura:set attribute="else">-->
                                <aura:if isTrue="{!not(or(v.BidAprrovalStatus=='Draft', and(or(v.BidAprrovalStatus=='SCM &amp; Marketing',v.BidAprrovalStatus=='Marketing'),and(and(v.isMarketingApprovePerson==v.loggedInUserName,v.hasDirectorLines==true),v.showProceedBtn==true))))}">
                                    
                                    <div class="slds-grid">
