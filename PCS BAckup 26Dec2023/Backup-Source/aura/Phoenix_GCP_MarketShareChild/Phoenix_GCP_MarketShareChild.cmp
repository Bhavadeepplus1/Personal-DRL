                            <aura:if isTrue="{!cmntLineList.cmntCat=='Vision Comments'}">
                                <lightning:accordionSection name="Vision Comments" label="Vision Comments">
                                    <aura:if isTrue="{!not(empty(cmntLineList.cmntWrapList))}">
