                                    <div class="slds-box userInputBox" style="width:1820px;background-color:white;padding-top:5px;">
                                        <aura:if isTrue="{!v.accGroupList.Vision_Segment__c=='Rx'}">
                                            <div class="slds-grid">
                                                
                                                <div class="slds-col slds-size_1-of-6" style="color:black" >
                                                    <lightning:input type="number"
                                                                     label="Min. DOH" 
                                                                     value="{!v.minRxDoH}"
                                                                     required="true"
                                                                     />
                                                </div>
                                                <div class="slds-col slds-size_1-of-6" style="padding-left:30px;color:black">
                                                    <lightning:input type="number"
                                                                     label="Max. DOH" 
                                                                     value="{!v.maxRxDoH}"
                                                                     required="true"
                                                                     />
                                                </div>
                                                <div class="slds-col slds-size_1-of-6" style="padding-left:30px;color:black">
                                                    <lightning:input type="number"  
                                                                     label="Min. Avg Sales"
                                                                     value="{!v.avgSalesValue}"
                                                                     />    
                                                </div>     
                                                <!--    <div class="slds-col slds-size_1-of-6" style="padding-left:30px;color:black">
                                                    <lightning:input type="number"  
                                                                     label="Threshold Multiplier"
                                                                     value="{!v.thresholdMtplr}"
                                                                     />    
                                                </div> -->
                                                <div class="slds-col slds-size_1-of-6" style="padding-left:30px;padding-top:24px;color:black">
                                                    
                                                    
                                                    <lightning:button title="cancel" 
                                                                      onclick="{!c.cancelChanges}"
                                                                      variant="neutral">
                                                        Cancel
                                                    </lightning:button>
                                                    
                                                     <aura:if isTrue="{!and(v.minRxDoH,v.maxRxDoH)}"> 
                                                    <lightning:button title="Save" 
                                                                      onclick="{!c.onSave}"
                                                                      variant="brand">
                                                        Save
                                                    </lightning:button>
                                                    </aura:if>
                                                </div>
                                            </div>
                                            
                                        </aura:if> 
                                        <aura:if isTrue="{!or(v.accGroupList.Vision_Segment__c=='SRx',v.accGroupList.Vision_Segment__c=='OTC')}">
                                            <div class="slds-grid">
                                                
                                                <div class="slds-col slds-size_1-of-6" style="color:black" >
                                                    <lightning:input type="number"
                                                                     label="Min. DOH" 
                                                                     value="{!v.minSRxDoH}"
                                                                     required="true"
                                                                     />
                                                </div>
                                                <div class="slds-col slds-size_1-of-6" style="padding-left:30px;color:black">
                                                    <lightning:input type="number"
                                                                     label="Max. DOH" 
                                                                     value="{!v.maxSRxDoH}"
                                                                     required="true"
                                                                     />
                                                </div>
                                                <div class="slds-col slds-size_1-of-6" style="padding-left:30px;color:black">
                                                    <lightning:input type="number"  
                                                                     label="Min. Avg Sales"
                                                                     value="{!v.avgSalesValue}"
                                                                     />    
                                                </div>     
                                                <!--   <div class="slds-col slds-size_1-of-6" style="padding-left:30px;color:black">
                                                    <lightning:input type="number"  
                                                                     label="Threshold Multiplier"
                                                                     value="{!v.thresholdMtplr}"
                                                                     required = "true"
                                                                     />    
                                                </div>    -->
                                                <div class="slds-col slds-size_1-of-6" style="padding-left:30px;padding-top:24px;color:black">
                                                    
                                                    
                                                    <lightning:button title="cancel" 
                                                                      onclick="{!c.cancelChanges}"
                                                                      variant="neutral">
                                                        Cancel
                                                    </lightning:button>
                                                    
                                                    <aura:if isTrue="{!and(v.minSRxDoH,v.maxSRxDoH)}"> 
                                                    <lightning:button title="Save" 
                                                                      onclick="{!c.onSave}"
                                                                      variant="brand">
                                                        Save
                                                    </lightning:button>
                                                    </aura:if>
                                                </div>
                                            </div>
                                            
                                        </aura:if>                        
                                    </div>
                                </aura:if>
                                <aura:set attribute="else">
                                    <lightning:buttonIcon class="lessThanCss" iconName="utility:chevrondown" variant="bare" onclick="{! c.handleClick2 }" name="{!v.accGroupList.Package_Description__c}" alternativeText="Show Inputs" title="Show inputs" size="x-large" />
                                </aura:set>
                                
                            </aura:if> 
                            
                        </div>
                        <div class="slds-col slds-size_1-of-6">
                            <b>WAC: </b><lightning:formattedNumber value="{!v.accGroupList.WAC_Price__c}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>
                        </div> 
                        
                        <!--   <div class="slds-col slds-size_5-of-12"></div> -->
                        
                    </div>
                    
                    
                    
                    
                    
                </div>
            </div>
        </td>        
    </tr>
    
    
    <tr id="styleids" style="font-weight: bold; background-color: white;text-align:center;"> 
        <aura:if isTrue="{!v.showRecords}"> 
                   
            <aura:if isTrue="{!v.dohIndicator}"> 
                
