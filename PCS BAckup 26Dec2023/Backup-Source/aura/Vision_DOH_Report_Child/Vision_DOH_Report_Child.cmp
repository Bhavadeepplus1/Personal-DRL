        <aura:iteration items="{!v.productFamilyDetails}" var="sdItem" indexVar="sNo">
            <tr id="styleids" style="font-weight: bold; background-color: white;text-align:center;"> 
                <aura:if isTrue="{!or(sdItem.W8_DOH__c == '',sdItem.W8_DOH__c == null)}">
                    <td class="dohAshIcon dohColumndohColumnIcon dohColumn" style="text-align:center;"><lightning:icon class="" iconName="utility:record" size="x-small" title="Present Week (W8) DoH data is provided as blank from Integrichain"/></td>
                </aura:if>
                <aura:if isTrue="{!v.accGroupList.Vision_Segment__c=='Rx'}">                    
                    <aura:if isTrue="{!(sdItem.W8_DOH__c le v.minRxDoHValue)}">
                        
                        <td class="redIcon dohColumndohColumnIcon dohColumn" style="text-align:center;"><lightning:icon class="" iconName="utility:dislike" size="x-small" title="Present Week (W8) DoH is NOT in between the Min DoH and Maz DoH set up by the user. "/></td>
                        
                    </aura:if>
                    <aura:if isTrue="{!(sdItem.W8_DOH__c ge v.maxRxDoHValue)}">
                        
                        <td class="redIcon dohColumnIcon dohColumn" style="text-align:center;"><lightning:icon class="" iconName="utility:dislike" size="x-small" title="Present Week (W8) DoH is NOT in between the Min DoH and Maz DoH set up by the user."/></td>
                        
                        
                    </aura:if>
                    <aura:if isTrue="{!and((sdItem.W8_DOH__c gt v.minRxDoHValue),(sdItem.W8_DOH__c lt v.maxRxDoHValue))}">
                        <td class="greenIcon dohColumnIcon dohColumn" style="text-align:center;"><lightning:icon class="" iconName="utility:like" size="x-small" title="Present Week (W8) DoH between the Min DoH and Maz DoH set up by the user."/></td>
                        
                        
                    </aura:if>
                    
                    
                </aura:if>
