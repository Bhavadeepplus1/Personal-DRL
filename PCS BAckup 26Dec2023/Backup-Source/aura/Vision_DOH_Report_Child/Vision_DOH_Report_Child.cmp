    <aura:if isTrue="{!v.showRecords}">
        <tr class=" styleids customTable">  
            
            <th scope="row" rowspan="2" style="{!((mod(v.indexValue,2)==0)  ? 'text-align:center;background-color:#FEB343;color:white' : 'text-align:center;background-color:#30AB30;color:white;')}" class="slds-cell-wrap dohColumn"  >
                <div class="slds-truncate ">
                    <aura:if isTrue="{!v.isAsc}">
                        <div class="slds-icon_container" style="padding: 8px;">  
                            <b style="color:white">DoH</b>&nbsp;<lightning:icon class="whiteIcon" iconName="utility:arrowup" 
                                                                                size="xx-small" 
                                                                                alternativeText="sort arrow up"
                                                                                onclick="{!c.sortByDOHredColor}"
                                                                                style="cursor: pointer"
                                                                                />  
                        </div>
                        
                        <aura:set attribute="else">                               
                            <div class="slds-icon_container" style="padding: 8px;">  
                                <b style="color:white;">DoH</b>&nbsp;<lightning:icon class="whiteIcon" iconName="utility:arrowdown" 
                                                                                     size="xx-small" 
                                                                                     alternativeText="sort arrow up"
                                                                                     onclick="{!c.sortByDOHGreenColor}"
                                                                                     style="cursor: pointer"
                                                                                     />  
                            </div>
                        </aura:set>
                    </aura:if>
                    
                    
                    
                </div>
