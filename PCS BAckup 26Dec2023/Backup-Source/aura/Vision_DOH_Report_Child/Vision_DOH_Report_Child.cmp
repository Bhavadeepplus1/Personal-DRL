                <td class="redIcon dohColumnIcon avgColumn" style="text-align:center;"><lightning:icon class="" iconName="utility:dislike" size="x-small"/></td>
                <aura:set attribute="else">
                    <aura:if isTrue="{!v.minAvgSalesdefault}">  
                        <td class="dohAshIcon dohColumnIcon avgColumn" style="text-align:center;"><lightning:icon class="" iconName="utility:record" size="x-small"/></td>
                        <aura:set attribute="else">
                            <td class="greenIcon dohColumnIcon avgColumn" style="text-align:center;"><lightning:icon class="" iconName="utility:like" size="x-small"/></td>
                        </aura:set>
                    </aura:if>
                </aura:set>   
            </aura:if>
            
            <td class="distributionColumnData distributionColumn"  style="text-align:center;"></td>
            <aura:set attribute="else">
                <td class="dohAshIcon dohColumnIcon dohColumn22" style="text-align:center;"><lightning:icon class="" iconName="utility:record" size="x-small"/></td>
                <!--  <td class="dohAshIcon dohColumnIcon qtySoldColumn2" style="text-align:center;"><lightning:icon class="" iconName="utility:record" size="x-small"/></td> -->
                <td class="dohAshIcon dohColumnIcon avgColumn22" style="text-align:center;"><lightning:icon class="" iconName="utility:record" size="x-small"/></td>
                <td class="distributionColumn22" style="text-align:center;"></td> 
                
                
            </aura:set>       
        </aura:if> 
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.ndcList.Vision_GCP_W1_DOH__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>               
        
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.accGroupList.W1_QOH__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>               
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.accGroupList.Value_W1__c}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/></td>
