        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.ndcList.Vision_GCP_W3_DOH__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>               
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.accGroupList.W3_QOH__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>               
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.accGroupList.Value_W3__c}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/></td>
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.accGroupList.W3_QTY_Sold__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>
        
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.ndcList.Vision_GCP_W4_DOH__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>               
        <td class=""  style="text-align:center;"><lightning:formattedNumber value="{!v.accGroupList.W4_QOH__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>               
