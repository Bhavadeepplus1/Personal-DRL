    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:if isTrue="{! v.loaded }">
        
        <center>
            
            <section role="dialog" aura:id="modalBlockUI" tabindex="-1" aria-labelledby="modal-heading-01"
                     aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">  
                <div class="slds-modal__container" style="background-Color: transparent; border: 0;">  
                    <div class="slds-modal__content slds-p-around_medium modalShadowRemove" style="background-Color: transparent; border: 0;">  
                        <img src="{!$Resource.LoadingProgress}" />  
                    </div>  
                </div>  
            </section>   
            <div class="slds-backdrop slds-backdrop-open" aura:id="customModal-Back"></div>
        </center>
    </aura:if>
    
    <tr id="styleids" style="font-weight: bold; background-color: #FEB343 ;text-align:center;color:white;">
        <td class="" colspan="40" style="{!((mod(v.indexValue,2)==0)  ? 'padding: 4px 8px;background-color:#FEB343;color:white' : 'padding: 4px 8px;background-color:#30AB30;color:white;')}">
            
            <div class="slds-grid">
                <div class="slds-col slds-size_12-of-12">
                    <div class="slds-grid">
                        <div class="slds-col slds-size_2-of-6">  
                            <b>{!v.accGroupList.Package_Description__c}</b>
                            &nbsp;
                            <b>-</b>&nbsp;
                            <b>{!v.accGroupList.NDC__c}</b>
                            &nbsp;&nbsp;&nbsp;
                            <aura:if isTrue="{!v.showRecords}">
                                <lightning:buttonIcon iconName="utility:minimize_window" class="IconColor iconCSS lessThanCss" variant="bare" onclick="{! c.hideRecords }" name="{!v.accGroupList.Package_Description__c}" alternativeText="Hide" title="Hide" size="x-large" />
                                
                                <aura:set attribute="else">
                                    <lightning:buttonIcon class="lessThanCss" iconName="utility:add" variant="bare" onclick="{! c.showRecords }" name="{!v.accGroupList.Package_Description__c}" alternativeText="Show" title="Show" size="x-large"/>
                                </aura:set>
                            </aura:if>
                            <aura:if isTrue="{!v.show}">
