<aura:component controller="VisionBidAnalysisCls" implements="force:lightningQuickActionWithoutHeader,force:appHostable,force:hasRecordId,force:hasSObjectName" access="global" >
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="scmData" type="List" />
    <aura:attribute name="gainedList" type="List" />
    <aura:attribute name="openStatusList" type="list" />
    <aura:attribute name="retainedGainList" type="List" />
    <aura:attribute name="retainedLossList" type="List" />
    <aura:attribute name="lossList" type="List" />
    <aura:attribute name="lossData" type="Object" />
    <aura:attribute name="noEffectList" type="List" />
    <aura:attribute name="categoryTotals" type="Object" />
    <aura:attribute name="bidNumber" type="string"/>
    <aura:attribute name="templateType" type="string"/>
    <aura:attribute name="totalLineItems" type="Integer" />
    <aura:attribute name="bidName" type="string"  />
    <aura:attribute name="title" type="String" />
    <aura:attribute name="isSpinnerLoad" type="boolean" default="false" />
    <aura:attribute name="totalDRlMargin"  type="Decimal" />
    <aura:attribute name="isClicked" type="boolean" default="false" />
    <aura:attribute name="EstimatedDaysFieldValues" type="List" default="0,7,15,20,30,45,60,75,90,120" />
    <aura:attribute name="SCMApprovalMap" type="Map" /> 
    <aura:attribute name="SCMApprovalQtyMap" type="Map" /> 
    <aura:attribute name="EstimatedDaysMap" type="Map" /> 
    <aura:attribute name="calculatedSCMQty" type="List" />
    <aura:attribute name="SCMReJMap" type="Map" /> 
    <aura:attribute name="SCMCommentsMap" type="Map" /> 
    <aura:attribute name="initailOrderVolumeMap" type="Map" /> 
    <aura:attribute name="initailOrderCommentsMap" type="Map" /> 
    <aura:attribute name="isSCMApproved" type="boolean" default="false" />
    <aura:attribute name="activeSections" type="List" />
    <aura:attribute name="businessGained" type="string"/>
    <aura:attribute name="retainedGain" type="string"/>
    <aura:attribute name="retainedLoss" type="string"/>
    <aura:attribute name="openStatus" type="string"/>
    <aura:attribute name="lost" type="string" default="Business Lost"/>
    <aura:attribute name="noEffect" type="string"/>
    <aura:attribute name="openLineItemsCount" type="Integer" />
    <aura:attribute name="RFPLossMap" type="Object" />
    <aura:attribute name="RFPLossFamilySummaryMap" type="Object" />
    <aura:attribute name="RFPLossList" type="List" />
    <aura:attribute name="gainedMap" type="Object" />
    <aura:attribute name="gainedFamilySummaryMap" type="Object" />
    <aura:attribute name="gainSummary" type="list" />
    <aura:attribute name="retainedGainMap" type="Object" />
    <aura:attribute name="retainedGainFamilySummaryMap" type="Object" />
    <aura:attribute name="retainedGainSummary" type="List" />
    <aura:attribute name="retainedLossMap" type="Object" />
    <aura:attribute name="retainedLossFamilySummaryMap" type="Object" />
    <aura:attribute name="retainedLossSummary" type="list" />
    <aura:attribute name="noEffectMap" type="Object" />
    <aura:attribute name="noEffectFamilySummaryMap" type="Object" />
    <aura:attribute name="noEffectSummary" type="list" />
    <aura:attribute name="openStatusMap" type="Object" />
    <aura:attribute name="openStatusFamilySummaryMap" type="Object" />
    <aura:attribute name="openStatusSummary" type="list" />
    
    <aura:attribute name="showPriceChangeDropDown" type="boolean" default="false"/>
    <aura:attribute name="showVolumeChangeDropDown" type="boolean" default="false" />
    <aura:attribute name="showBothChangeDropDown" type="boolean" default="false" />
    <aura:attribute name="dynamicMargin" type="Decimal" default="192" />
    
    <aura:method name="totalImpactMethod" action="{!c.getParams}" access="public">
        <aura:attribute name="totalAnnualImpact" type="integer" />
        <aura:attribute name="totalBusinessImpact" type="integer" />
    </aura:method>
    <!--added by har for approvals-->
    <aura:attribute name="isSCMApprovePerson" type="boolean" /> 
    <!--added by har for approvals-->
    
    <aura:attribute name="showUserInputs" type="boolean" default="false"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/> 
    <aura:attribute name="scmRejactionReasonValues" type="List" default="FG Quality Issue, API Quality Issue, Raw Material/KSM Quality issue, Packing Material Quality Issue, API Supply Delay, Raw Material/KSM Supply issue, Packing Material Supply Issue, Manufacturing Delay, FG capacity constrained, API capacity constrained, Low FG Inventory on Hand, Others"/>
    <aura:method name="scmRefresh" action="{!c.doInit}" />
    <aura:method name="onloadSCM" action="{!c.onLoadingSCM}" />
    <aura:attribute name="bidRecord" type="Object" />
    
    <aura:html tag="style">
        .tableDiv{
        height : auto !important;
        max-height: 400px !important;
        overflow : scroll !important;
        }
    </aura:html>
    <lightning:card>
        <aura:set attribute="title">
            <lightning:icon iconName="custom:custom90" size="small"/>&nbsp;&nbsp;
            {! v.title }&nbsp;
            <aura:if isTrue="{!and(v.categoryTotals.totalAnnualImpact != null, v.categoryTotals.totalAnnualImpact != 0)}">
                <aura:if isTrue="{!greaterthan(v.categoryTotals.totalAnnualImpact, 0)}">
                    <span style="color: green;" title="Annual Impact, FY Business Impact">(<lightning:formattedNumber value="{!v.categoryTotals.totalAnnualImpact}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>, <lightning:formattedNumber value="{!v.categoryTotals.totalBusinessImpact}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>)</span>
                    <aura:set attribute="else">
                        <span style="color: red;" title="Annual Impact, FY Business Impact">(<lightning:formattedNumber value="{!v.categoryTotals.totalAnnualImpact}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>, <lightning:formattedNumber value="{!v.categoryTotals.totalBusinessImpact}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>)</span>
                    </aura:set>
                </aura:if>
            </aura:if>
        </aura:set> 
        <aura:set attribute="actions">
            <lightning:button label="Back" onclick="{! c.onClose}" />
        </aura:set>
        <div aura:id="mainDiv" class=" slds-m-right_small slds-m-left_small" style="height:auto;">
            <aura:if isTrue="{!v.isSpinnerLoad}">
                <div class="backgroundInverse">
                    <lightning:spinner alternativeText="Loading" variant="brand" />
                </div>
            </aura:if>
            <aura:if isTrue="{!v.scmData.length > 0}" >
                <aura:if isTrue="{!v.isClicked}">
                    <div class="slds-align_absolute-center slds-m-top_small slds-m-bottom_small">        
                        <lightning:button variant="neutral" label="Cancel" onclick="{! c.refreshSCM}" />      
                        <lightning:button variant="brand" label="Save" onclick="{! c.saveClick }" />
                    </div>
                </aura:if>
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="{! v.activeSections }" >
                    <lightning:accordionSection name="Summary" label="Summary">
                        <div style="margin: 0px 0px 16px 0px;font-weight: bold;">
                            <div style="width: 30%; display:inline-block;margin: 4px 8px 4px 0px;border: 1px solid #cdcdcd;border-radius: 6px;padding-bottom: 30px;">
                                <div style="padding: 8px;">
                                    <table class="slds-box slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered slds-no-row-hover" style="width: 100%;font-size: 16px;padding: 0px;">
                                        <thead class="headerOfTable">
                                            <tr style="background-color:#6f30a0;">
                                                <th colspan="2" style="text-align: left;background-color:#6f30a0;color: white;">Annual Sales</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Current</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.totalPreviousTotal != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.totalPreviousTotal, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber title="Current Sales" value="{!v.categoryTotals.totalPreviousTotal}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.totalPreviousTotal, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber title="Current Sales" value="{!v.categoryTotals.totalPreviousTotal}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.totalPreviousTotal, 0)}">
                                                            <lightning:formattedNumber value="{!v.categoryTotals.totalPreviousTotal}" title="Current Sales" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; 
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Awarded</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.totalCurrentTotal != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.totalCurrentTotal, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber title="Awarded Sales" value="{!v.categoryTotals.totalCurrentTotal}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.totalCurrentTotal, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber title="Awarded Sales" value="{!v.categoryTotals.totalCurrentTotal}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.totalCurrentTota, 0)}">
                                                            <lightning:formattedNumber value="{!v.categoryTotals.totalCurrentTotal}" title="Awarded Sales" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; 
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Variance</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.change != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.change, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber title="Awarded Sales - Current Sales" value="{!v.categoryTotals.change}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.change, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber title="Awarded Sales - Current Sales" value="{!v.categoryTotals.change}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.change, 0)}">
                                                            <lightning:formattedNumber value="{!v.categoryTotals.change}" title="Awarded Sales - Current Sales" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Variance(%)</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!and(v.categoryTotals.percentageChange != 'Infinity', v.categoryTotals.percentageChange != null)}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.percentageChange, 0)}">
                                                            <span style="color: green;" title="(Awarded Sales - Current Sales)/Current Sales"><i>{!v.categoryTotals.percentageChange}%</i>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.percentageChange, 0)}">
                                                            <span style="color: red;" title="(Awarded Sales - Current Sales)/Current Sales"><i>{!v.categoryTotals.percentageChange}%</i>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.percentageChange, 0)}">
                                                            <span title="(Awarded Sales - Current Sales)/Current Sales"><i>{!v.categoryTotals.percentageChange}%</i>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            <i>N/A</i>&nbsp;
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div style="padding: 8px;">
                                    <table class="slds-box slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered slds-no-row-hover" style="font-size: 16px;padding: 0px;">
                                        <thead class="headerOfTable">
                                            <tr>
                                                <th colspan="2" style="text-align: left;background-color:#6f30a0;color: white;">Variance</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Price</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.priceVariance != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.priceVariance, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber title="(Awarded Deadnet - Current Deadnet)*Awarded Quantity" value="{!v.categoryTotals.priceVariance}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.priceVariance, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber title="(Awarded Deadnet - Current Deadnet)*Awarded Quantity" value="{!v.categoryTotals.priceVariance}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.priceVariance, 0)}">
                                                            <lightning:formattedNumber title="(Awarded Deadnet - Current Deadnet)*Awarded Quantity" value="{!v.categoryTotals.priceVariance}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Volume</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.volumeVariance != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.volumeVariance, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber value="{!v.categoryTotals.volumeVariance}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.volumeVariance, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber value="{!v.categoryTotals.volumeVariance}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.volumeVariance, 0)}">
                                                            <lightning:formattedNumber value="{!v.categoryTotals.volumeVariance}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div style="padding: 8px;">
                                    <table class="slds-box slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered slds-no-row-hover" style="font-size: 16px;padding: 0px;">
                                        <thead class="headerOfTable">
                                            <tr>
                                                <th colspan="2" style="text-align: left;background-color:#6f30a0;color: white;">Fiscal Sales Impact</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Current</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.totalCurrentFiscalImpact != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.totalCurrentFiscalImpact, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber value="{!v.categoryTotals.totalCurrentFiscalImpact}" title="(Current Sales * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.totalCurrentFiscalImpact, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber value="{!v.categoryTotals.totalCurrentFiscalImpact}" title="(Current Sales * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.totalCurrentFiscalImpact, 0)}">
                                                            <lightning:formattedNumber value="{!v.categoryTotals.totalCurrentFiscalImpact}" title="(Current Sales * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; 
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Awarded</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.totalProposedFiscalImpact != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.totalProposedFiscalImpact, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber value="{!v.categoryTotals.totalProposedFiscalImpact}" title="(Awarded Sales * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.totalProposedFiscalImpact, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber value="{!v.categoryTotals.totalProposedFiscalImpact}" title="(Awarded Sales * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.totalProposedFiscalImpact, 0)}">
                                                            <lightning:formattedNumber value="{!v.categoryTotals.totalProposedFiscalImpact}" title="(Awarded Sales * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; 
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Variance</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.salesFiscalVar != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.salesFiscalVar, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber  title="Awarded Fiscal Sales Impact - Current Fiscal Sales Impact" value="{!v.categoryTotals.salesFiscalVar}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.salesFiscalVar, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber title="Awarded Fiscal Sales Impact - Current Fiscal Sales Impact" value="{!v.categoryTotals.salesFiscalVar}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.salesFiscalVar, 0)}">
                                                            <lightning:formattedNumber title="Awarded Fiscal Sales Impact - Current Fiscal Sales Impact" value="{!v.categoryTotals.salesFiscalVar}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Variance(%)</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!and(v.categoryTotals.salesFiscalVarPer != 'Infinity', v.categoryTotals.salesFiscalVarPer != null)}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.salesFiscalVarPer, 0)}">
                                                            <span style="color: green;" title="(Awarded Fiscal Sales Impact - Current Fiscal Sales Impact)/Current Fiscal Sales Impact" ><i>{!v.categoryTotals.salesFiscalVarPer}%</i>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.salesFiscalVarPer, 0)}">
                                                            <span style="color: red;" title="(Awarded Fiscal Sales Impact - Current Fiscal Sales Impact)/Current Fiscal Sales Impact"><i>{!v.categoryTotals.salesFiscalVarPer}%</i>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.salesFiscalVarPer, 0)}">
                                                            <span title="(Awarded Fiscal Sales Impact - Current Fiscal Sales Impact)/Current Fiscal Sales Impact"><i>{!v.categoryTotals.salesFiscalVarPer}%</i>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            <i>N/A</i>&nbsp;
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            
                            <div style="width: 30%; display:inline-block;margin: 4px 8px 4px 0px;border: 1px solid #cdcdcd;border-radius: 6px;">
                                <div style="padding: 8px;">
                                    <table class="slds-box slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered slds-no-row-hover" style="font-size: 16px;padding: 0px;">
                                        <thead class="headerOfTable">
                                            <tr>
                                                <th colspan="2" style="text-align: left;background-color:#6f30a0;color: white;">TPT</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td style="text-align: left;width: 50%;">Current($)</td>
                                                <td style="text-align: left;width: 50%;">
                                                    <aura:if isTrue="{!v.categoryTotals.previousTPT != null}">
                                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.previousTPT, 0)}">
                                                            <span style="color: green;"><lightning:formattedNumber value="{!v.categoryTotals.previousTPT}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!lessthan(v.categoryTotals.previousTPT, 0)}">
                                                            <span style="color: red;"><lightning:formattedNumber value="{!v.categoryTotals.previousTPT}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                                        </aura:if>
                                                        <aura:if isTrue="{!equals(v.categoryTotals.previousTPT, 0)}">
                                                            <lightning:formattedNumber value="{!v.categoryTotals.previousTPT}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;
                                                        </aura:if>
                                                        <aura:set attribute="else">
                                                            N/A
                                                        </aura:set>
                                                    </aura:if>
                                                </td>
