<aura:component controller="VisionQRMBidAnalysisCls" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <aura:attribute name="Q1NewProductLaunchList" type="list" />
    <aura:attribute name="Q2NewProductLaunchList" type="list" />
    <aura:attribute name="Q1ProActiveList" type="list" />
    <aura:attribute name="Q2ProActiveList" type="list" />
    <aura:attribute name="Q1ReActiveList" type="list" />
    <aura:attribute name="Q2ReActiveList" type="list" />
    <aura:attribute name="Q1ROFRsList" type="list" />
    <aura:attribute name="Q2ROFRsList" type="list" />
    <aura:attribute name="salesObj" type="Object" />
    <aura:attribute name="awardedObj" type="Object" />
    <aura:attribute name="familyKeys" type="list" />
    <aura:attribute name="NewCustomerBidsList" type="list" />
    <aura:attribute name="ProductAdditionBidsList" type="list" />
    <aura:attribute name="isSpinnerLoad" type="boolean" default="false" />
    <aura:attribute name="selectedYear" type="integer" />
    <aura:attribute name="dataLoaded" type="boolean" />
    <aura:attribute name="selectedQ1" type="string" />
    <aura:attribute name="selectedQ2" type="string" />
    <aura:attribute name="Q1Label" type="string" />
    <aura:attribute name="Q2Label" type="string" />
    <aura:attribute name="loadingMessage" type="string" />
    <aura:attribute name="selectedOption" type="string" />
    <aura:attribute name="basedOn" type="String" default="Closed Date"/>
    <aura:attribute name="Quarter1" type="Object" />
    <aura:attribute name="Quarter2" type="Object" />
    <aura:attribute name="Quarter3" type="Object" />
    <aura:attribute name="Quarter4" type="Object" />
    <aura:attribute name="Quarter5" type="Object" />
    <aura:attribute name="BidsCountObj" type="Object" />
    <aura:attribute name="E1Object" type="Object" />
    <aura:attribute name="showE1View" type="boolean" default="false" />
    <aura:attribute name="isChecked" type="boolean" default="false" />
    <aura:attribute name="viewName" type="string" default="E-1" />
    <aura:attribute name="popupData" type="list" />
    <aura:attribute name="isModalOpen" type="boolean" />
    <aura:attribute name="selectedQuarter" type="String" />
    <aura:attribute name="selectedCategory" type="String" />
    <aura:attribute name="declinedFamilyKeys" type="list" />
    <aura:attribute name="showAwarded" type="boolean" />
    <aura:attribute name="showDeclined" type="boolean" />
    <aura:attribute name="finalAwardedSummaryObj" type="Object" />
    <aura:attribute name="finalDeclinedSummaryObj" type="Object" />
    <aura:attribute name="proActiveWinRate" type="Decimal" />
    <aura:attribute name="options" type="List" default="[
                                                        {'label': 'Closed Date', 'value': 'Closed Date'},
                                                        {'label': 'Submitted Date', 'value': 'Submitted Date'},
                                                        {'label': 'Customer Response Date', 'value': 'Customer Response Date'}
                                                        ]"/>
    <!--<aura:attribute name="options" type="List" default="[
                                                        {'label': 'Submitted Date', 'value': 'Submitted Date'},
                                                        {'label': 'Customer Response Date', 'value': 'Customer Response Date'},
                                                        {'label': 'Closed Date', 'value': 'Closed Date'},
                                                        {'label': 'Submitted Date/Customer Response Date', 'value': 'Submitted Date/Customer Response Date'},
                                                        {'label': 'Submitted Date/Closed Date', 'value': 'Submitted Date/Closed Date'},
                                                        {'label': 'Customer Response Date/Closed Date', 'value': 'Customer Response Date/Closed Date'},
                                                        {'label': 'Submitted Date/Customer Response Date/Closed Date', 'value': 'Submitted Date/Customer Response Date/Closed Date'},
                                                        ]"/>-->
    <aura:attribute name="quarters" type="List" />
    <!--<aura:attribute name="years" type="List" default="[
                                                        {'label': '2023', 'value': '2023'},
                                                        {'label': '2022', 'value': '2022'},
                                                        {'label': '2021', 'value': '2021'},
                                                        {'label': '2020', 'value': '2020'},
                                                        ]"/>-->
    
    <aura:handler name = "init" value ="{!this}" action = "{!c.doInit}"/>
    <lightning:tabset selectedTabId="one">
        <lightning:tab label="Executive Summary View" id="one">
            <aura:if isTrue="{!v.isSpinnerLoad}">
                <center>
                    <section role="dialog" aura:id="modalBlockUI" tabindex="-1" aria-labelledby="modal-heading-01"
                             aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">  
                        <div class="slds-modal__container" style="background-Color: transparent; border: 0;">  
                            <div class="slds-modal__content slds-p-around_medium modalShadowRemove" style="background-Color: transparent; border: 0;">  
                                <img src="{!$Resource.LoadingProgress}" />  
                            </div>  
                        </div>  
                    </section>   
                    <div class="slds-backdrop slds-backdrop-open" aura:id="customModal-Back"></div>
                </center>
            </aura:if>
            <div class="slds-box" style="padding: 4px; font-size: 13px;background-color: #d0e7f7;">
                <div class="slds-grid">
                    <div class="slds-col slds-size_1-of-8" style="margin: 4px;">
                        <lightning:combobox name="Based On" label="Based On" value="{!v.basedOn}" placeholder="Enter Time Period" options="{! v.options }" />
                    </div>
                    <!--<div class="slds-col slds-size_1-of-8" style="margin: 4px;">
                        <lightning:combobox name="Select Year" label="Select Year" value="{!v.selectedYear}" placeholder="Select Year" options="{! v.years }" onchange="{! c.handleYearChange }"/>
                    </div>-->
                    <div class="slds-col slds-size_1-of-8" style="margin: 4px;">
                        <lightning:combobox name="Select Quarter" label="Select Quarter" value="{!v.selectedQ1}" placeholder="Select Quarter" options="{! v.quarters }" onchange="{! c.handleQ1Change }"/>
                    </div>
                    <div class="slds-col slds-size_1-of-8" style="margin: 4px;">
                        <lightning:combobox name="Select Quarter to Compare" label="Select Quarter to Compare" value="{!v.selectedQ2}" placeholder="Select Quarter to Compare" options="{! v.quarters }" onchange="{! c.handleQ2Change }"/>
                    </div>
                    <div style="margin-top: 28px;">
                        <lightning:button variant="brand" label="Compare" title="Compare" onclick="{! c.handleClick }" disabled="{!or(v.selectedQ1 == null, v.selectedQ2 == null)}"/>
                    </div>
                </div>
            </div>
            <aura:if isTrue="{!v.isModalOpen}">
                <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_small" aria-labelledby="modal-heading-01" aria-modal="true">
                    <div class="slds-modal__container">
                        <header class="slds-modal__content_headless" style="text-align: right;">
                            <lightning:buttonIcon iconName="utility:close"
                                                  onclick="{! c.closeModal }"
                                                  alternativeText="close"
                                                  variant="bare-inverse"
                                                  class="slds-modal__close buttonCSS"/>
                        </header>
                        
                        <div class="slds-modal__content slds-p-around_medium slds-is-relative" id="modal-content-id-1" style="padding-left:10px;">
                            <div style="padding:5px;">
                                <lightning:icon iconName="utility:arrowup" size="xx-small" class="greenIcon"/>&nbsp;-&nbsp;% Win Rate increases by >=10% relative to the quarter being compared.<br/>
                                <lightning:icon iconName="utility:arrowdown" size="xx-small" class="redIcon"/>&nbsp;-&nbsp;% Win Rate decreases by >=10% relative to the quarter being compared.<br/>
                                <span style="font-weight: bold;color: gray;font-size: 16px;">~</span>&nbsp;-&nbsp;% Win Rate is between (+)/(-)10% relative to the quarter being compared.<br/>
                                &nbsp;&nbsp;&nbsp;&nbsp;% Win Rate is the ratio of “bids won to the total no. of bids in any quarter” expressed in % terms.<br/>
                                &nbsp;&nbsp;&nbsp;&nbsp;Proactive bids include 1) all share expansion 'Product Addition' bids other than RFPs and, 2) bids sent to new customers.<br/>
                                &nbsp;&nbsp;&nbsp;&nbsp;Reactive bids include 1) RFP share expansions, 2) OTBs &amp; volume review bids.<br/>
                                &nbsp;&nbsp;&nbsp;&nbsp;ROFRs include price reduction bids (RFPs and others).<br/>
                                &nbsp;&nbsp;&nbsp;&nbsp;Awarded sales in case of GPOs could be overstated, as compliance (% on usage) tend to be lower for these.<br/>
                            </div> 
                        </div>
                    </div>
                    
                </section>
                <div class="slds-backdrop slds-backdrop_open" role="presentation"></div>
            </aura:if>
            <aura:if isTrue="{!v.dataLoaded}">
                <div class="slds-box" style="padding: 4px 4px 0px 4px;margin-top:8px;">
                    <div style="margin-bottom: 4px;text-align: center;font-size: 16px;padding: 4px;background-color: #025196;color: white;font-weight: bold;border-radius: 4px;">
                        Bid statistic comparison between {!v.Q2Label} and {!v.Q1Label}
                    </div>
                    <div style="display: inline-block;margin-right: 4px;font-size: 13px;width: 49%;">
                        <!--<div style="font-weight: bold;padding: 4px; font-size: 16px;text-align: center;">{!v.Q2Label}</div>-->
                        <table class="slds-box slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered slds-no-row-hover" style="margin-bottom: 4px;padding: 0px;">
                            <thead class="headerOfTable">
                                <tr style="background-color:#27708b;">
                                    <th style="background-color:#27708b;color: white;text-align: center; padding-left: 0px;" width="25%">{!v.Q2Label}</th>
                                    <th style="background-color:#27708b;color: white;text-align: center;" width="25%">No. of Bids</th>
                                    <th style="background-color:#27708b;color: white;text-align: center;" width="25%">Sales $ Mn</th>
                                    <th style="background-color:#27708b;color: white;text-align: center;" width="25%">% Win Rate</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.NPLQ2Click}">New Product Launches (NPLs)</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q2NPLBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q2SalesNPL/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.awardedObj.Q2NPLWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.ProActiveQ2Click}">Proactive Bids</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q2ProActiveBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q2SalesProActive/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.awardedObj.Q2ProActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.ReActiveQ2Click}">Reactive Bids</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q2ReActiveBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q2SalesReActive/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.awardedObj.Q2ReActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.ROFRQ2Click}">Right of First Refusals (ROFRs)</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q2ROFRBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q2SalesROFRs/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.awardedObj.Q2ROFRsWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                    </td>
                                </tr>
                                <tr style="background: #b5ccd2; font-weight: bold;">
                                    <td style="padding-left: 6px;text-align: center;">Total</td>
                                    <td style="text-align: center;">{!(v.BidsCountObj.Q2NPLBidsCount+v.BidsCountObj.Q2ProActiveBidsCount+v.BidsCountObj.Q2ReActiveBidsCount+v.BidsCountObj.Q2ROFRBidsCount)}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q2Sales/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q2WinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div style="display: inline-block;margin-left: 4px;font-size: 13px;width: 49%;float: right; ">
                        <!--<div style="font-weight: bold;padding: 4px; font-size: 16px;text-align: center;">{!v.Q1Label}</div>-->
                        <table class="slds-box slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered slds-no-row-hover" style="margin-bottom: 4px;padding: 0px;">
                            <thead class="headerOfTable">
                                <tr style="background-color:#27708b;">
                                    <th style="background-color:#27708b;color: white;text-align: center; padding-left: 0px;" width="25%">{!v.Q1Label}</th>
                                    <th style="background-color:#27708b;color: white;text-align: center;" width="25%">No. of Bids</th>
                                    <th style="background-color:#27708b;color: white;text-align: center;" width="25%">Sales $ Mn</th>
                                    <th style="background-color:#27708b;color: white;text-align: center;" width="25%">% Win Rate</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.NPLQ1Click}">New Product Launches (NPLs)</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q1NPLBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q1SalesNPL/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <aura:if isTrue="{!v.awardedObj.greaterNPL}">
                                        <td style="text-align: center;">
                                            <lightning:formattedNumber value="{!v.awardedObj.Q1NPLWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                            <lightning:icon iconName="utility:arrowup" size="xx-small" class="greenIcon" title="{!v.awardedObj.hoverNPLMessage}" />
                                        </td>
                                        <aura:set attribute="else">
                                            <aura:if isTrue="{!v.awardedObj.lesserNPL}">
                                                <td style="text-align: center;">
                                                    <lightning:formattedNumber value="{!v.awardedObj.Q1NPLWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small" title="{!v.awardedObj.hoverNPLMessage}" class="redIcon" />
                                                </td>
                                                <aura:set attribute="else">
                                                    <td style="text-align: center;padding-top: 0px;padding-bottom: 2px;">
                                                        <lightning:formattedNumber value="{!v.awardedObj.Q1NPLWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                        <aura:if isTrue="{!v.awardedObj.isNaNNPL}">
                                                            <aura:set attribute="else">
                                                                <span style="font-weight: bold;color: gray;font-size: 16px;" title="{!v.awardedObj.hoverNPLMessage}">~</span>
                                                            </aura:set>
                                                        </aura:if>
                                                    </td>
                                                </aura:set>
                                            </aura:if>
                                        </aura:set>
                                    </aura:if>                
                                </tr>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.ProActiveQ1Click}">Proactive Bids</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q1ProActiveBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q1SalesProActive/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <aura:if isTrue="{!v.awardedObj.greaterProActive}">
                                        <td style="text-align: center;">
                                            <lightning:formattedNumber value="{!v.awardedObj.Q1ProActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                            <lightning:icon iconName="utility:arrowup" size="xx-small" class="greenIcon" title="{!v.awardedObj.hoverProActiveMessage}" />
                                        </td>
                                        <aura:set attribute="else">
                                            <aura:if isTrue="{!v.awardedObj.lesserProActive}">
                                                <td style="text-align: center;">
                                                    <lightning:formattedNumber value="{!v.awardedObj.Q1ProActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small" title="{!v.awardedObj.hoverProActiveMessage}" class="redIcon" />
                                                </td>
                                                <aura:set attribute="else">
                                                    <td style="text-align: center;padding-top: 0px;padding-bottom: 2px;">
                                                        <lightning:formattedNumber value="{!v.awardedObj.Q1ProActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                        <aura:if isTrue="{!v.awardedObj.isNaNProActive}">
                                                            <aura:set attribute="else">
                                                                <span style="font-weight: bold;color: gray;font-size: 16px;" title="{!v.awardedObj.hoverProActiveMessage}">~</span>
                                                            </aura:set>
                                                        </aura:if>
                                                    </td>
                                                </aura:set>
                                            </aura:if>
                                        </aura:set>
                                    </aura:if>  
                                </tr>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.ReActiveQ1Click}">Reactive Bids</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q1ReActiveBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q1SalesReActive/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    
                                    <aura:if isTrue="{!v.awardedObj.greaterReActive}">
                                        <td style="text-align: center;">
                                            <lightning:formattedNumber value="{!v.awardedObj.Q1ReActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                            <lightning:icon iconName="utility:arrowup" size="xx-small" class="greenIcon" title="{!v.awardedObj.hoverReActiveMessage}" />
                                        </td>
                                        <aura:set attribute="else">
                                            <aura:if isTrue="{!v.awardedObj.lesserReActive}">
                                                <td style="text-align: center;">
                                                    <lightning:formattedNumber value="{!v.awardedObj.Q1ReActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small" title="{!v.awardedObj.hoverReActiveMessage}" class="redIcon" />
                                                </td>
                                                <aura:set attribute="else">
                                                    <td style="text-align: center;padding-top: 0px;padding-bottom: 2px;">
                                                        <lightning:formattedNumber value="{!v.awardedObj.Q1ReActiveWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                        <aura:if isTrue="{!v.awardedObj.isNaNReActive}">
                                                            <aura:set attribute="else">
                                                                <span style="font-weight: bold;color: gray;font-size: 16px;" title="{!v.awardedObj.hoverReActiveMessage}">~</span>
                                                            </aura:set>
                                                        </aura:if>
                                                    </td>
                                                </aura:set>
                                            </aura:if>
                                        </aura:set>
                                    </aura:if>
                                </tr>
                                <tr>
                                    <td style="padding-left: 6px;font-weight: bold;background-color: #e3f2ed;"><i><span style="cursor: pointer;" onclick="{!c.ROFRQ1Click}">Right of First Refusals (ROFRs)</span></i></td>
                                    <td style="text-align: center;">{!v.BidsCountObj.Q1ROFRBidsCount}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q1SalesROFRs/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    <aura:if isTrue="{!v.awardedObj.greaterROFRs}">
                                        <td style="text-align: center;">
                                            <lightning:formattedNumber value="{!v.awardedObj.Q1ROFRsWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                            <lightning:icon iconName="utility:arrowup" size="xx-small" class="greenIcon" title="{!v.awardedObj.hoverROFRsMessage}" />
                                        </td>
                                        <aura:set attribute="else">
                                            <aura:if isTrue="{!v.awardedObj.lesserROFRs}">
                                                <td style="text-align: center;">
                                                    <lightning:formattedNumber value="{!v.awardedObj.Q1ROFRsWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small" title="{!v.awardedObj.hoverROFRsMessage}" class="redIcon" />
                                                </td>
                                                <aura:set attribute="else">
                                                    <td style="text-align: center;padding-top: 0px;padding-bottom: 2px;">
                                                        <lightning:formattedNumber value="{!v.awardedObj.Q1ROFRsWinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                                        <aura:if isTrue="{!v.awardedObj.isNaNROFRs}">
                                                            <aura:set attribute="else">
                                                                <span style="font-weight: bold;color: gray;font-size: 16px;" title="{!v.awardedObj.hoverROFRsMessage}">~</span>
                                                            </aura:set>
                                                        </aura:if>
                                                    </td>
                                                </aura:set>
                                            </aura:if>
                                        </aura:set>
                                    </aura:if>
                                </tr>
                                <tr style="background: #b5ccd2; font-weight: bold;">
                                    <td style="padding-left: 6px;text-align: center;">Total</td>
                                    <td style="text-align: center;">{!(v.BidsCountObj.Q1NPLBidsCount+v.BidsCountObj.Q1ProActiveBidsCount+v.BidsCountObj.Q1ReActiveBidsCount+v.BidsCountObj.Q1ROFRBidsCount)}</td>
                                    <td style="text-align: center;">
                                        <lightning:formattedNumber value="{!v.salesObj.Q1Sales/1000000}" minimumFractionDigits="0" maximumFractionDigits="1"/>
                                    </td>
                                    
                                    <aura:if isTrue="{!v.salesObj.greaterRate}">
                                        <td style="text-align: center;">
                                            <lightning:formattedNumber value="{!v.salesObj.Q1WinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
                                            <lightning:icon iconName="utility:arrowup" size="xx-small" class="greenIcon" title="{!v.salesObj.hoverTotalMessage}" />
                                        </td>
                                        <aura:set attribute="else">
                                            <aura:if isTrue="{!v.salesObj.lesserRate}">
                                                <td style="text-align: center;">
                                                    <lightning:formattedNumber value="{!v.salesObj.Q1WinRate}" style="percent" minimumFractionDigits="0" maximumFractionDigits="0"/>
