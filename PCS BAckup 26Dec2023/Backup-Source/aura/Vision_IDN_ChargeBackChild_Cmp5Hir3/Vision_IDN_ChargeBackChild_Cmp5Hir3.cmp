        <td class="slds-text-align_right" style="{!(OR(AND(v.novchargeBackAmount == 0,v.octchargeBackAmount==0),AND(v.novchargeBackAmount == 0,v.decchargeBackAmount==0)) ? 'color:red;text-align:right;' : 'color:black;')}"><lightning:formattedNumber value="{!v.novchargeBackAmount}" style="currency" currencyCode="USD" maximumFractionDigits="0" /></td>
         <td class="slds-text-align_right" style="{!(OR(AND(v.col2Units == 0,v.col1Units==0),AND(v.col2Units == 0,v.col3Units==0)) ? 'color:red;text-align:right;' : 'color:black;')}">{!v.col2Units}</td>
