                            <lightning:icon class="redIcon" iconName="utility:arrowdown"
                                            size="xx-small"
                                            alternativeText="sort arrow up"
                                            onmouseover="{!c.comparingUnits}"
                                            style="cursor: pointer"    
                                            title="{! 'Previous Total of 6 Month :' + v.prev6monthsUnits
                                                   + ' '+ 'and' +' '
