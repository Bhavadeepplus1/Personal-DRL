<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <aura:attribute name="record" type="object" />
    <aura:attribute name="family" type="String" />
    <aura:attribute name="index" type="integer" />
    <aura:attribute name="totalAnnualImpact" type="integer" />
    <aura:attribute name="totalBusinessImpact" type="integer" />
    <aura:attribute name="showUserInputs" type="boolean" default="true"/>
    <aura:attribute name="category" type="String" />
    <aura:attribute name="bidRecord" type="Object" />
    <aura:attribute name="summaryForLost" type="Object" />
    <aura:attribute name="showTooltip" type="boolean" default="false" />
    <aura:attribute name="isBidBusinessAnalysis" type="boolean" />
    <aura:attribute name="top" type="string" default="-22px;"/>
    <aura:attribute name="size" type="Integer"/>
    <aura:attribute name="isLastRows" type="boolean" />
    <aura:attribute name="expandFamily" type="boolean" />
    <aura:attribute name="previousTotal" type="Integer"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <tr>
        <td style="padding: 4px 8px; text-align: right;">
            <aura:if isTrue="{!v.record.showDropdown}">
                <a onclick="{!c.showDropDown}">
                    <lightning:icon iconName="utility:chevronup" size="x-small" />
                </a>
                <aura:set attribute="else">
                    <a onclick="{!c.showDropDown}">
                        <lightning:icon iconName="utility:chevrondown" size="x-small" />
                    </a>
                </aura:set>
            </aura:if>
        </td>
        <td style="text-align: left;">
            {!v.record.productName}
        </td>
        <td style="text-align: left;">
            {!v.record.customer}
        </td>
        <td style="text-align: left;">
            <div>
                <aura:if isTrue="{!v.record.bidStatus}">
                    <lightning:formattedUrl value="{!'/' + v.record.bidRecord.Id}" label="{!v.record.bidStatus}" target="_blank" onmouseover="{!c.mouseOver}" onmouseout="{!c.mouseOut}" />
                </aura:if>
                <aura:if isTrue="{!and(v.showTooltip, v.isBidBusinessAnalysis)}">
                    <div class="{!v.isLastRows ? 'slds-popover slds-popover_tooltip slds-nubbin_left-bottom': 'slds-popover slds-popover_tooltip slds-nubbin_left-top'}" role="tooltip" id="help" style="{!'top:'+(v.top)+'width:auto;position:absolute;left:160px;max-width: 600px;'}">
                        <div class="slds-popover__body" style="width: auto;">
                            <tr style="border: 0;">
                                <td style="border: 0;padding-left: 0px;">Bid No</td>
                                <td style="border: 0;padding-left: 0px;">:</td>
                                <td class="slds-cell-wrap" style="border: 0; min-width: 400px;"><i><b>{!v.record.bidRecord.Name}</b></i></td>
                            </tr>
                            <tr style="border: 0;">
                                <td style="border: 0;padding-left: 0px;">Bid Name</td>
                                <td style="border: 0;padding-left: 0px;">:</td>
                                <td class="slds-cell-wrap" style="border: 0;"><i><b>{!v.record.bidRecord.Phoenix_Bid_Name__c}</b></i></td>
                            </tr>
                            <tr style="border: 0;">
                                <td style="border: 0;padding-left: 0px;">Bid Owner</td>
                                <td style="border: 0;padding-left: 0px;">:</td>
                                <td style="border: 0;"><i><b>{!v.record.bidRecord.Phoenix_Bid_Owner__r.Name}</b></i></td>
                            </tr>
                            <tr style="border: 0;">
                                <td style="border: 0;padding-left: 0px;">Customer</td>
                                <td style="border: 0;padding-left: 0px;">:</td>
                                <td class="slds-cell-wrap" style="border: 0;"><i><b>{!v.record.bidRecord.Phoenix_Customer__r.Name}</b></i></td>
                            </tr>
                            <tr style="border: 0;">
                                <td style="border: 0;padding-left: 0px;">Bid Submitted Date</td>
                                <td style="border: 0;padding-left: 0px;">:</td>
                                <td style="border: 0;"><i><b>{!v.record.formattedSubmittedDate}</b></i></td>
                            </tr>
                            <tr style="border: 0;">
                                <td style="border: 0;padding-left: 0px;">Bid Closed Date</td>
                                <td style="border: 0;padding-left: 0px;">:</td>
                                <td style="border: 0;"><i><b>{!v.record.formattedClosedDate}</b></i></td>
                            </tr>
                        </div>
                    </div>
                </aura:if>
            </div>
        </td>
        <aura:if isTrue="{!v.record.monthsRemaining}">
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.monthsRemaining}" maximumFractionDigits="2" title="{!v.record.impactedTitle}"/>&nbsp; <span title="{!v.record.date}">({!v.record.dateUsed})</span>
            </td>
            <aura:set attribute="else">
                <td style="text-align: center;">
                </td>
            </aura:set>
        </aura:if>
        <td style="text-align: center;" title="{!v.record.contractTotalPreviousTitle}">
            <aura:if isTrue="{!and(v.record.previousContractTotal != null, v.record.previousContractTotal != 0)}">
                <ui:outputCurrency aura:id="curr" value="{!v.record.previousContractTotal}" format="$##,##,###,###"/>
                <aura:set attribute="else">
                    <aura:if isTrue="{!or(v.record.previousContractTotal == 0, v.record.previousContractTotal == null)}">
                        $0
                    </aura:if>
                </aura:set>
            </aura:if>
        </td>
        <td style="text-align: center;" title="{!v.record.contractTotalCurrentTitle}">
            <aura:if isTrue="{!and(v.record.currentContractTotal != null, v.record.currentContractTotal != 0)}">
                <ui:outputCurrency aura:id="curr" value="{!v.record.currentContractTotal}" format="$##,##,###,###"/>
                <aura:set attribute="else">
                    <aura:if isTrue="{!or(v.record.currentContractTotal == 0, v.record.currentContractTotal == null)}">
                        $0
                    </aura:if>
                </aura:set>
            </aura:if>
        </td>
        <td style="text-align: center;">
            <aura:if isTrue="{!and(v.record.annualImpact != 0, v.record.annualImpact != null)}">
                <aura:if isTrue="{!greaterthan(v.record.annualImpact, 0)}">
                    <span style="color: green;" title="{!v.record.annualTitle}"><ui:outputCurrency aura:id="curr" value="{!v.record.annualImpact}" format="$##,##,###,###"/></span>
                </aura:if>
                <aura:if isTrue="{!lessthan(v.record.annualImpact, 0)}">
                    <span style="color: red;" title="{!v.record.annualTitle}"><ui:outputCurrency aura:id="curr" value="{!v.record.annualImpact}" format="$##,##,###,###"/></span>                    
                </aura:if>
                <aura:set attribute="else">
                    <span title="{!v.record.annualTitle}">$0</span>
                </aura:set>
            </aura:if>
        </td>
        <td style="text-align: center;">
            <aura:if isTrue="{!and(v.record.businessImpact != 0, v.record.businessImpact != null)}">
                <aura:if isTrue="{!greaterthan(v.record.businessImpact, 0)}">
                    <span style="color: green;" title="{!v.record.businessTitle}"><ui:outputCurrency aura:id="curr" value="{!v.record.businessImpact}" format="$##,##,###,###"/></span>
                </aura:if>
                <aura:if isTrue="{!lessthan(v.record.annualImpact, 0)}">
                    <span style="color: red;" title="{!v.record.businessTitle}"><ui:outputCurrency aura:id="curr" value="{!v.record.businessImpact}" format="$##,##,###,###"/></span>                    
                </aura:if>
                <aura:set attribute="else">
                    <span title="{!v.record.businessTitle}">$0</span>
                </aura:set>
            </aura:if>
        </td>
        <aura:if isTrue="{!v.bidRecord.Phoenix_Customer_Type__c == 'ClarusOne'}">
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.overrideCurrentDirectUnits}" maximumFractionDigits="2"/>
            </td>
            <td style="text-align: center;">
                <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                    <lightning:formattedNumber value="{!v.record.osUnits}" maximumFractionDigits="2"/>
                    <aura:set attribute="else">
                        Combined
                    </aura:set>
                </aura:if>
            </td>
            <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                <td style="text-align: center;">
                    <lightning:formattedNumber value="{!v.record.radUnits}" maximumFractionDigits="2"/>
                </td>
            </aura:if>
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_ProposedIndirectAholdDelhaizeUni__c}" minimumFractionDigits="0" maximumFractionDigits="0"/>
            </td>
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_Proposed_IndirectGaintEagleUnits__c}" minimumFractionDigits="0" maximumFractionDigits="0"/>
            </td>
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.overrideCurrentIndirectUnits}" maximumFractionDigits="2"/>
            </td>
            <td style="text-align: center;">
                <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.record.businessCategory == 'No Effect')}">
                    <lightning:formattedNumber value="{!v.record.radUnits}" maximumFractionDigits="2"/>
                    <aura:set attribute="else">
                        Combined
                    </aura:set>
                </aura:if>
            </td>
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.currentRetailDirectUnits}" maximumFractionDigits="2"/>
            </td>
            <td style="text-align: center;">
                <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                    <lightning:formattedNumber value="{!v.record.wmtUnits}" maximumFractionDigits="2"/>
                    <aura:set attribute="else">
                        Combined
                    </aura:set>
                </aura:if>
            </td>
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_Final_Direct_Selling_Units_Calc__c}" minimumFractionDigits="0" maximumFractionDigits="0"/>
            </td>
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!(v.record.overrideCurrentDirectUnits + v.record.overrideCurrentIndirectUnits + v.record.currentRetailDirectUnits)}" maximumFractionDigits="2"/>
            </td>
            <td style="text-align: center;">
                <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                    <lightning:formattedNumber value="{!(v.record.osUnits + v.record.radUnits + v.record.wmtUnits)}" maximumFractionDigits="2"/>
                    <aura:if isTrue="{!greaterthan((v.record.osUnits + v.record.radUnits + v.record.wmtUnits), (v.record.overrideCurrentDirectUnits + v.record.overrideCurrentIndirectUnits + v.record.currentRetailDirectUnits))}">
                        <span style="margin-left: 4px;">
                            <lightning:icon iconName="utility:arrowup" class="greenIcon" size="xx-small" />
                        </span>
                    </aura:if>
                    <aura:if isTrue="{!lessthan((v.record.osUnits + v.record.radUnits + v.record.wmtUnits), (v.record.overrideCurrentDirectUnits + v.record.overrideCurrentIndirectUnits + v.record.currentRetailDirectUnits))}">
                        <span style="margin-left: 4px;">
                            <lightning:icon iconName="utility:arrowdown" class="redIcon" size="xx-small" />
                        </span>
                    </aura:if>
                    <aura:if isTrue="{!equals((v.record.osUnits + v.record.radUnits + v.record.wmtUnits), (v.record.overrideCurrentDirectUnits + v.record.overrideCurrentIndirectUnits + v.record.currentRetailDirectUnits))}">
                        <span style="margin-left: 4px;">
                            <lightning:icon iconName="utility:assignment" class="greenIcon" size="xx-small" />
                        </span>
                    </aura:if>
                    <aura:set attribute="else">
                        <lightning:formattedNumber value="{!v.record.awardedQty}" maximumFractionDigits="2"/>
                        <aura:if isTrue="{!greaterthan(v.record.awardedQty, (v.record.overrideCurrentDirectUnits + v.record.overrideCurrentIndirectUnits + v.record.currentRetailDirectUnits))}">
                            <span style="margin-left: 4px;">
                                <lightning:icon iconName="utility:arrowup" class="greenIcon" size="xx-small" />
                            </span>
                        </aura:if>
                        <aura:if isTrue="{!lessthan(v.record.awardedQty, (v.record.overrideCurrentDirectUnits + v.record.overrideCurrentIndirectUnits + v.record.currentRetailDirectUnits))}">
                            <span style="margin-left: 4px;">
                                <lightning:icon iconName="utility:arrowdown" class="redIcon" size="xx-small" />
                            </span>
                        </aura:if>
                        <aura:if isTrue="{!equals(v.record.awardedQty, (v.record.overrideCurrentDirectUnits + v.record.overrideCurrentIndirectUnits + v.record.currentRetailDirectUnits))}">
                            <span style="margin-left: 4px;">
                                <lightning:icon iconName="utility:assignment" class="greenIcon" size="xx-small" />
                            </span>
                        </aura:if>
                    </aura:set>
                </aura:if>
            </td>
            <td style="text-align: center;">
                <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_OS_RAD_NCP__c}" style="currency" currencyCode="USD" maximumFractionDigits="2"/>
            </td>
            <td style="text-align: center;">
                <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                    <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_OS_RAD_NCP__c}" style="currency" currencyCode="USD" maximumFractionDigits="2"/>
                    <aura:set attribute="else">
                        <aura:if isTrue="{!and(or(v.bidRecord.Phoenix_Bid_Type__c == 'Volume Review Only',  v.bidRecord.Phoenix_Bid_Type__c == 'Sales Out Rebate'), or(v.record.bidLineItem.Phoenix_Proposed_McK_OS_And_RAD_NCP__c == 0, v.record.bidLineItem.Phoenix_Proposed_McK_OS_And_RAD_NCP__c == null))}">
                            <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_OS_RAD_NCP__c}" style="currency" currencyCode="USD" maximumFractionDigits="2"/>
                            <span style="margin-left: 4px;">
                                <lightning:icon iconName="utility:assignment" class="greenIcon" title="0" size="xx-small" />
                            </span>
                            <aura:set attribute="else">
                                <ui:outputCurrency aura:id="curr" value="{!v.record.bidLineItem.Phoenix_Proposed_McK_OS_And_RAD_NCP__c}" format="$##,##,###,###.00"/>
                                <aura:if isTrue="{!greaterthan(v.record.bidLineItem.Phoenix_Proposed_McK_OS_And_RAD_NCP__c, v.record.bidLineItem.Phoenix_OS_RAD_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:arrowup" class="greenIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                                <aura:if isTrue="{!lessthan(v.record.bidLineItem.Phoenix_Proposed_McK_OS_And_RAD_NCP__c, v.record.bidLineItem.Phoenix_OS_RAD_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:arrowdown" class="redIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                                <aura:if isTrue="{!equals(v.record.bidLineItem.Phoenix_Proposed_McK_OS_And_RAD_NCP__c, v.record.bidLineItem.Phoenix_OS_RAD_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:assignment" class="greenIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                            </aura:set>
                        </aura:if>
                    </aura:set>
                </aura:if>
            </td>
            <td style="text-align: center;">
                <ui:outputCurrency aura:id="curr" value="{!v.record.bidLineItem.Phoenix_WMT_Direct_NCP__c}" format="$##,##,###,###.00"/>
            </td>
            <td style="text-align: center;">
                <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                    <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_WMT_Direct_NCP__c}" style="currency" currencyCode="USD" maximumFractionDigits="2"/>
                    <aura:set attribute="else">
                        <aura:if isTrue="{!and(or(v.bidRecord.Phoenix_Bid_Type__c == 'Volume Review Only',  v.bidRecord.Phoenix_Bid_Type__c == 'Sales Out Rebate'), or(v.record.bidLineItem.Phoenix_Proposed_WMT_Direct_NCP__c == 0, v.record.bidLineItem.Phoenix_Proposed_WMT_Direct_NCP__c == null))}">
                            <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_WMT_Direct_NCP__c}" style="currency" currencyCode="USD" maximumFractionDigits="2"/>
                            <span style="margin-left: 4px;">
                                <lightning:icon iconName="utility:assignment" class="greenIcon" title="0" size="xx-small" />
                            </span>
                            <aura:set attribute="else">
                                <ui:outputCurrency aura:id="curr" value="{!v.record.bidLineItem.Phoenix_Proposed_WMT_Direct_NCP__c}" format="$##,##,###,###.00"/>
                                <aura:if isTrue="{!greaterthan(v.record.bidLineItem.Phoenix_Proposed_WMT_Direct_NCP__c, v.record.bidLineItem.Phoenix_WMT_Direct_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:arrowup" class="greenIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                                <aura:if isTrue="{!lessthan(v.record.bidLineItem.Phoenix_Proposed_WMT_Direct_NCP__c, v.record.bidLineItem.Phoenix_WMT_Direct_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:arrowdown" class="redIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                                <aura:if isTrue="{!equals(v.record.bidLineItem.Phoenix_Proposed_WMT_Direct_NCP__c, v.record.bidLineItem.Phoenix_WMT_Direct_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:assignment" class="greenIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                            </aura:set>
                        </aura:if>
                    </aura:set>
                </aura:if>
            </td>
            <td style="text-align: center;">
                <ui:outputCurrency aura:id="curr" value="{!v.record.bidLineItem.Phoenix_WMT_Indirect_NCP__c}" format="$##,##,###,###.00"/>
            </td>
            <td style="text-align: center;">
                <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                    <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_WMT_Indirect_NCP__c}" style="currency" currencyCode="USD" maximumFractionDigits="2"/>
                    <aura:set attribute="else">
                        <aura:if isTrue="{!and(or(v.bidRecord.Phoenix_Bid_Type__c == 'Volume Review Only',  v.bidRecord.Phoenix_Bid_Type__c == 'Sales Out Rebate'), or(v.record.bidLineItem.Phoenix_Proposed_WMT_Indirect_NCP__c == 0, v.record.bidLineItem.Phoenix_Proposed_WMT_Indirect_NCP__c == null))}">
                            <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_WMT_Indirect_NCP__c}" style="currency" currencyCode="USD" maximumFractionDigits="2"/>
                            <span style="margin-left: 4px;">
                                <lightning:icon iconName="utility:assignment" class="greenIcon" title="0" size="xx-small" />
                            </span>
                            <aura:set attribute="else">
                                <ui:outputCurrency aura:id="curr" value="{!v.record.bidLineItem.Phoenix_Proposed_WMT_Indirect_NCP__c}" format="$##,##,###,###.00"/>
                                <aura:if isTrue="{!greaterthan(v.record.bidLineItem.Phoenix_Proposed_WMT_Indirect_NCP__c, v.record.bidLineItem.Phoenix_WMT_Indirect_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:arrowup" class="greenIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                                <aura:if isTrue="{!lessthan(v.record.bidLineItem.Phoenix_Proposed_WMT_Indirect_NCP__c, v.record.bidLineItem.Phoenix_WMT_Indirect_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:arrowdown" class="redIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                                <aura:if isTrue="{!equals(v.record.bidLineItem.Phoenix_Proposed_WMT_Indirect_NCP__c, v.record.bidLineItem.Phoenix_WMT_Indirect_NCP__c)}">
                                    <span style="margin-left: 4px;">
                                        <lightning:icon iconName="utility:assignment" class="greenIcon" size="xx-small" />
                                    </span>
                                </aura:if>
                            </aura:set>
                        </aura:if>
                    </aura:set>
                </aura:if>
            </td>
            <aura:set attribute="else">
                <aura:if isTrue="{!v.bidRecord.Phoenix_Customer_Type__c == 'RXSS'}">
                    <td style="text-align: center;">
                        <lightning:formattedNumber value="{!v.record.overrideCurrentDirectUnits}" maximumFractionDigits="2"/>
                    </td>
                    <td style="text-align: center;">
                        <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                            <lightning:formattedNumber value="{!v.record.directAholDelhaizeUnits}" maximumFractionDigits="2"/>
                            <aura:set attribute="else">
                                Combined
                            </aura:set>
                        </aura:if>
                    </td>
                    <td style="text-align: center;">
                        <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                            <lightning:formattedNumber value="{!v.record.directGaintEagleUnits}" maximumFractionDigits="2"/>
                            <aura:set attribute="else">
                                Combined
                            </aura:set>
                        </aura:if>
                    </td>
                    <td style="text-align: center;">
                        <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_Day_s_Notice_Product_removal__c}" maximumFractionDigits="0" minimumFractionDigits="0"/>
                    </td>
                    <td style="text-align: center;">
                        <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_Current_Indirect_Gaint_EagleUnit__c}" maximumFractionDigits="0" minimumFractionDigits="0"/>
                    </td>
                    <td style="text-align: center;">
                        <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_Current_Retail_Indirect_Units__c}" maximumFractionDigits="2"/>
                    </td>
                    <td style="text-align: center;">
                        <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                            <lightning:formattedNumber value="{!v.record.retailIndirectUnits}" maximumFractionDigits="2"/>
                            <aura:set attribute="else">
                                Combined
                            </aura:set>
                        </aura:if>
                    </td>
                    <td style="text-align: center;">
                        <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_Proposed_IndirectGaintEagleUnits__c}" maximumFractionDigits="0" minimumFractionDigits="0"/>
                    </td>
                    <td style="text-align: center;">
                        <lightning:formattedNumber value="{!v.record.overrideCurrentIndirectUnits}" maximumFractionDigits="2"/>
                    </td>
                    <td style="text-align: center;">
                        <aura:if isTrue="{!or(v.record.businessCategory == 'Open Status', v.category == 'No Effect')}">
                            <lightning:formattedNumber value="{!v.record.proposedSmithDrugUnits}" maximumFractionDigits="2"/>
                            <aura:set attribute="else">
                                Combined
                            </aura:set>
                        </aura:if>
                    </td>
                    <td style="text-align: center;">
                        <lightning:formattedNumber value="{!v.record.bidLineItem.Phoenix_IDN_Usage__c}" maximumFractionDigits="0" minimumFractionDigits="0"/>
