                        <tbody>
                            <tr>
                                <td style="text-align: left;width: 50%;">Current</td>
                                <td style="text-align: left;width: 50%;">
                                    <aura:if isTrue="{!v.categoryTotals.totalCurrentTPTFiscalImpact != null}">
                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.totalCurrentTPTFiscalImpact, 0)}">
                                            <span style="color: green;"><lightning:formattedNumber value="{!v.categoryTotals.totalCurrentTPTFiscalImpact}" title="(Current TPT($) * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                        </aura:if>
                                        <aura:if isTrue="{!lessthan(v.categoryTotals.totalCurrentTPTFiscalImpact, 0)}">
                                            <span style="color: red;"><lightning:formattedNumber value="{!v.categoryTotals.totalCurrentTPTFiscalImpact}" title="(Current TPT($) * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                        </aura:if>
                                        <aura:if isTrue="{!equals(v.categoryTotals.totalCurrentTPTFiscalImpact, 0)}">
                                            <lightning:formattedNumber value="{!v.categoryTotals.totalCurrentTPTFiscalImpact}" title="(Current TPT($) * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; 
                                        </aura:if>
                                        <aura:set attribute="else">
                                            N/A
                                        </aura:set>
                                    </aura:if>
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: left;width: 50%;">Awarded</td>
                                <td style="text-align: left;width: 50%;">
                                    <aura:if isTrue="{!v.categoryTotals.totalProposedTPTFiscalImpact != null}">
                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.totalProposedTPTFiscalImpact, 0)}">
                                            <span style="color: green;"><lightning:formattedNumber value="{!v.categoryTotals.totalProposedTPTFiscalImpact}" title="(Awarded TPT($) * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                        </aura:if>
                                        <aura:if isTrue="{!lessthan(v.categoryTotals.totalProposedTPTFiscalImpact, 0)}">
                                            <span style="color: red;"><lightning:formattedNumber value="{!v.categoryTotals.totalProposedTPTFiscalImpact}" title="(Awarded TPT($) * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; </span>
                                        </aura:if>
                                        <aura:if isTrue="{!equals(v.categoryTotals.totalProposedTPTFiscalImpact, 0)}">
                                            <lightning:formattedNumber value="{!v.categoryTotals.totalProposedTPTFiscalImpact}" title="(Awarded TPT($) * Impacted Months)/12" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp; 
                                        </aura:if>
                                        <aura:set attribute="else">
                                            N/A
                                        </aura:set>
                                    </aura:if>
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: left;width: 50%;">Variance</td>
                                <td style="text-align: left;width: 50%;">
                                    <aura:if isTrue="{!v.categoryTotals.tptFiscalVar != null}">
                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.tptFiscalVar, 0)}">
                                            <span style="color: green;" title="Awarded Fiscal TPT Impact - Current Fiscal TPT Impact"><lightning:formattedNumber value="{!v.categoryTotals.tptFiscalVar}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                        </aura:if>
                                        <aura:if isTrue="{!lessthan(v.categoryTotals.tptFiscalVar, 0)}">
                                            <span style="color: red;" title="Awarded Fiscal TPT Impact - Current Fiscal TPT Impact"><lightning:formattedNumber value="{!v.categoryTotals.tptFiscalVar}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;</span>
                                        </aura:if>
                                        <aura:if isTrue="{!equals(v.categoryTotals.tptFiscalVar, 0)}">
                                            <lightning:formattedNumber title="Awarded Fiscal TPT Impact - Current Fiscal TPT Impact" value="{!v.categoryTotals.tptFiscalVar}" style="currency" currencyCode="USD" minimumFractionDigits="0" maximumFractionDigits="0"/>&nbsp;
                                        </aura:if>
                                        <aura:set attribute="else">
                                            N/A
                                        </aura:set>
                                    </aura:if>
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: left;width: 50%;">Variance(%)</td>
                                <td style="text-align: left;width: 50%;">
                                    <aura:if isTrue="{!and(v.categoryTotals.tptFiscalVarPer != 'Infinity', v.categoryTotals.tptFiscalVarPer != null)}">
                                        <aura:if isTrue="{!greaterthan(v.categoryTotals.tptFiscalVarPer, 0)}">
                                            <span title="(Awarded Fiscal TPT Impact - Current Fiscal TPT Impact)/Current Fiscal TPT Impact" style="color: green;"><i>{!v.categoryTotals.tptFiscalVarPer}%</i>&nbsp;</span>
                                        </aura:if>
                                        <aura:if isTrue="{!lessthan(v.categoryTotals.tptFiscalVarPer, 0)}">
                                            <span title="(Awarded Fiscal TPT Impact - Current Fiscal TPT Impact)/Current Fiscal TPT Impact" style="color: red;"><i>{!v.categoryTotals.tptFiscalVarPer}%</i>&nbsp;</span>
                                        </aura:if>
                                        <aura:if isTrue="{!equals(v.categoryTotals.tptFiscalVarPer, 0)}">
                                            <span title="(Awarded Fiscal TPT Impact - Current Fiscal TPT Impact)/Current Fiscal TPT Impact"><i>{!v.categoryTotals.tptFiscalVarPer}%</i>&nbsp;</span>
                                        </aura:if>
                                        <aura:set attribute="else">
                                            <i>N/A</i>&nbsp;
                                        </aura:set>
                                    </aura:if>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div id="inner3">  
            <div style="margin: 4px 8px 4px 0px;border: 1px solid #cdcdcd;border-radius: 6px;padding-bottom: 30px;">
                <div style="padding: 8px;">
                    <table class="slds-box slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered slds-no-row-hover" style="font-size: 16px;padding: 0px;">
                        <thead class="headerOfTable">
                            <tr>
                                <th colspan="1" style="text-align: left;background-color:#6f30a0;color: white;">Count</th>
                                <th style="text-align: right;background-color:#6f30a0;color: white;border-left: none;">
                                    <lightning:buttonIcon class="chartIcon" variant="bare-inverse" size="large" iconName="utility:chart" alternativeText="Generate Pie Chart" title="Generate Pie Chart" onclick="{!c.displayPieChart}" />
                                    <lightning:buttonIcon class="chartIcon" variant="bare-inverse" size="large" iconName="utility:metrics" alternativeText="" title="" onclick="{!c.displayWaterfallChart}" />
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td style="text-align: left;width: 50%;"># Bid Line Item (Bids)</td>
                                <td style="text-align: left;width: 50%;">
                                    <!--This count should be checked again and use this variable {!v.totalLineItems}-->
                                    {!(v.openLineItemsCount + v.gainedList.length + v.categoryTotals.deadnetChangesCount + v.categoryTotals.awardedQtyChangesCount + v.categoryTotals.bothPriceAndVolumeChangesCount + v.lossList.length + v.noEffectList.length)} (<i>{!v.totalBids}</i>)
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: left;width: 50%;"># Open Status(s)</td>
                                <td style="text-align: left;width: 50%;">
                                    {!v.openLineItemsCount} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentOpen}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: left;width: 50%;"># New Award(s)</td>
                                <td style="text-align: left;width: 50%;">
                                    {!v.gainedList.length} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentNewAwards}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: left;width: 50%;"># Price Change(s)
                                    <aura:if isTrue="{!v.showPriceChangeDropDown}">
                                        <a onclick="{!c.showPriceChangeDropDown}">
                                            <lightning:icon iconName="utility:chevronup" size="x-small" />
                                        </a>
                                        <aura:set attribute="else">
                                            <a onclick="{!c.showPriceChangeDropDown}">
                                                <lightning:icon iconName="utility:chevrondown" size="x-small" />
                                            </a>
                                        </aura:set>
                                    </aura:if>
                                </td>
                                <td style="text-align: left;width: 50%;">
                                    {!v.categoryTotals.deadnetChangesCount} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentPriceChanges}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                </td>
                            </tr>
                            <aura:if isTrue="{!v.showPriceChangeDropDown}">
                                <tr>
                                    <td style="text-align: left;width: 70%; padding-left: 50px;"># Business Retained with Gains</td>
                                    <td style="text-align: left;width: 30%; padding-left: 50px;">
                                        {!v.categoryTotals.priceChangeListRetainedWithGains} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentPriceChangeListRetainedWithGains}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                    </td>
                                </tr>
                                <tr>
                                    <td style="text-align: left;width: 70%; padding-left: 50px;"># Business Retained with Loss</td>
                                    <td style="text-align: left;width: 30%; padding-left: 50px;">
                                        {!v.categoryTotals.priceChangeListRetainedWithLoss} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentPriceChangeListRetainedWithLoss}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                    </td>
                                </tr>
                            </aura:if>
                            <tr>
                                <td style="text-align: left;width: 50%;"># Volume Change(s)
                                    <aura:if isTrue="{!v.showVolumeChangeDropDown}">
                                        <a onclick="{!c.showVolumeChangeDropDown}">
                                            <lightning:icon iconName="utility:chevronup" size="x-small" />
                                        </a>
                                        <aura:set attribute="else">
                                            <a onclick="{!c.showVolumeChangeDropDown}">
                                                <lightning:icon iconName="utility:chevrondown" size="x-small" />
                                            </a>
                                        </aura:set>
                                    </aura:if>
                                </td>
                                <td style="text-align: left;width: 50%;">
                                    {!v.categoryTotals.awardedQtyChangesCount} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentQtyChanges}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                </td>
                            </tr>
                            <aura:if isTrue="{!v.showVolumeChangeDropDown}">
                                <tr>
                                    <td style="text-align: left;width: 70%; padding-left: 50px;"># Business Retained with Gains</td>
                                    <td style="text-align: left;width: 30%; padding-left: 50px;">
                                        {!v.categoryTotals.volumeChangeListRetainedWithGains} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentVolumeChangeListRetainedWithGains}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                    </td>
                                </tr>
                                <tr>
                                    <td style="text-align: left;width: 70%; padding-left: 50px;"># Business Retained with Loss</td>
                                    <td style="text-align: left;width: 30%; padding-left: 50px;">
                                        {!v.categoryTotals.volumeChangeListRetainedWithLoss} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentVolumeChangeListRetainedWithLoss}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                    </td>
                                </tr>
                            </aura:if>
                            <tr>
                                <td style="text-align: left;width: 50%;"># Price &amp; Volume Change(s)
                                    <aura:if isTrue="{!v.showBothChangeDropDown}">
                                        <a onclick="{!c.showBothChangeDropDown}">
                                            <lightning:icon iconName="utility:chevronup" size="x-small" />
                                        </a>
                                        <aura:set attribute="else">
                                            <a onclick="{!c.showBothChangeDropDown}">
                                                <lightning:icon iconName="utility:chevrondown" size="x-small" />
                                            </a>
                                        </aura:set>
                                    </aura:if>
                                </td>
                                <td style="text-align: left;width: 50%;">
                                    {!v.categoryTotals.bothPriceAndVolumeChangesCount} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentBothChanges}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                </td>
                            </tr>
                            <aura:if isTrue="{!v.showBothChangeDropDown}">
                                <tr>
                                    <td style="text-align: left;width: 70%; padding-left: 50px;"># Business Retained with Gains</td>
                                    <td style="text-align: left;width: 30%; padding-left: 50px;">
                                        {!v.categoryTotals.bothListRetainedWithGains} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentBothListRetainedWithGains}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                    </td>
                                </tr>
                                <tr>
                                    <td style="text-align: left;width: 70%; padding-left: 50px;"># Business Retained with Loss</td>
                                    <td style="text-align: left;width: 30%; padding-left: 50px;">
                                        {!v.categoryTotals.bothListRetainedWithLoss} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentBothListRetainedWithLoss}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                    </td>
                                </tr>
                            </aura:if>
                            <tr>
                                <td style="text-align: left;width: 50%;"># Product Lost(s)</td>
                                <td style="text-align: left;width: 50%;">
                                    {!v.lossList.length} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentLoss}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: left;width: 50%;"># No Effect(s)</td>
                                <td style="text-align: left;width: 50%;">
                                    {!v.noEffectList.length} (<i><lightning:formattedNumber value="{!v.categoryTotals.percentNoEffects}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="2"/></i>)
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</aura:component>