                </div>
                <div class="modal-body scrollable slds-modal__content slds-p-around--medium" data-scoped-scroll="true" style="background-color:white;padding:2px 2px 2px 2px;">
                    <div class="slds-modal__content slds-p-around_large" style="padding:0px 0px 0px 0px;">
                        <aura:if isTrue="{!v.showSpinner}">
                            <lightning:spinner variant="brand" class="spinColor" size="large" />
                        </aura:if>
