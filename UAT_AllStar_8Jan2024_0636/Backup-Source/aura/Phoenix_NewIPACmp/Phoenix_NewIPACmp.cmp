                        <div class="matchPanelError" tabindex="0"  role="alert" style="color:#c23934;text-align:center;">
                            <span>Please enter IDN Usage value for all selected products</span>
                        </div>
                        
                    </aura:if>
                    <aura:if isTrue="{!v.errPrice}">
                        <div class="matchPanelError" tabindex="0"  role="alert" style="color:#c23934;text-align:center;">
                            <span>Please enter Proposed IPA Price for all selected products</span>
                        </div>
                        
                    </aura:if>
