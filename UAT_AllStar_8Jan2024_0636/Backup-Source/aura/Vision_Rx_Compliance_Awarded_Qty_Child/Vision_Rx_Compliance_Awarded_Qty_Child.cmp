                        <td style="text-align:center;"><lightning:formattedNumber value="{!singlerec.CTD_Total_Awarded_Quantity__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>
                        <td style="text-align:center;"><lightning:formattedNumber value="{!singlerec.CTD_Total_Quantity__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>
                    </aura:if>
                    <td class="" style="{!(singlerec.CTD_Total_Compliance__c ge 90 ? 'color:purple;text-align:center;' : AND(singlerec.CTD_Total_Compliance__c le 90, singlerec.CTD_Total_Compliance__c ge 75) ? 'color:green;text-align:center;' : 'color:red;text-align:center;')}">
                        <lightning:formattedNumber value="{!singlerec.CTD_Total_Compliance__c}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="0"/> </td>          
