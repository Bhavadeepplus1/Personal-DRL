                                                                                                  'purplelikeIcon slds-text-align_center MTDColumnData' )}"><lightning:icon iconName="utility:like" size="x-small" title="{!singlerec.MTD_Total_Compliance__c}"/></td>
                        <aura:set attribute="else"> 
                            <td style="border-right-style: solid;border-right-color: #cac4c2;" class="redIcon MTDColumnData" ><lightning:icon iconName="utility:dislike" title="{!singlerec.MTD_Total_Compliance__c }" size="x-small"/></td>
                            
                        </aura:set>
                    </aura:if> 
                    
                    <aura:if isTrue="{!or(singlerec.M7_Total_Compliance__c ge 75,singlerec.M7_Total_Compliance__c == undefined )}">          
                        <td  class="{!(AND(singlerec.M7_Total_Compliance__c lt 90, singlerec.M7_Total_Compliance__c ge 75) ? 'greenIcon slds-text-align_center month1ColumnData' :
                                    'purplelikeIcon slds-text-align_center month1ColumnData' )}"><lightning:icon iconName="utility:like" size="x-small" title="{!singlerec.M7_Total_Compliance__c}"/></td>
                        <aura:set attribute="else"> 
                            <td class="redIcon month1ColumnData" ><lightning:icon iconName="utility:dislike" title="{!singlerec.M7_Total_Compliance__c }" size="x-small"/></td>
                            
                        </aura:set>
                    </aura:if> 
                    <aura:if isTrue="{!or(singlerec.M6_Total_Compliance__c ge 75,singlerec.M6_Total_Compliance__c == undefined )}">          
                        <td  class="{!(AND(singlerec.M6_Total_Compliance__c lt 90, singlerec.M6_Total_Compliance__c ge 75) ? 'greenIcon slds-text-align_center month2ColumnData' :
                                    'purplelikeIcon slds-text-align_center month2ColumnData' )}"><lightning:icon iconName="utility:like" size="x-small" title="{!singlerec.M6_Total_Compliance__c}"/></td>
                        <aura:set attribute="else"> 
                            <td class="redIcon month2ColumnData" ><lightning:icon iconName="utility:dislike" title="{!singlerec.M6_Total_Compliance__c }" size="x-small"/></td>
                            
                        </aura:set>
                    </aura:if> 
                    <aura:if isTrue="{!or(singlerec.M5_Total_Compliance__c ge 75,singlerec.M5_Total_Compliance__c == undefined )}">          
