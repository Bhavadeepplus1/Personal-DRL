                            <td class="redIcon FTDColumnData" ><lightning:icon iconName="utility:dislike" title="{!singlerec.FTD_Total_Compliance__c }" size="x-small"/></td>
                            
                        </aura:set>
                    </aura:if> 
                    <aura:if isTrue="{!or(singlerec.CTD_Total_Compliance__c ge 75,singlerec.CTD_Total_Compliance__c == undefined )}">          
