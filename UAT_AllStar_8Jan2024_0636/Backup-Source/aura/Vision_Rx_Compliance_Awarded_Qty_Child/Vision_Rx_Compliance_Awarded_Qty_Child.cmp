                        <td style="text-align:center;"><lightning:formattedNumber value="{!singlerec.QTD_Total_Quantity__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>
                    </aura:if>
                    <td class="" style="{!(singlerec.QTD_Total_Complaince__c ge 90 ? 'color:purple;text-align:center;' : AND(singlerec.QTD_Total_Complaince__c le 90, singlerec.QTD_Total_Complaince__c ge 75) ? 'color:green;text-align:center;' : 'color:red;text-align:center;')}">
                        <lightning:formattedNumber value="{!singlerec.QTD_Total_Complaince__c}" style="percent-fixed" minimumFractionDigits="0" maximumFractionDigits="0"/> </td>                 
