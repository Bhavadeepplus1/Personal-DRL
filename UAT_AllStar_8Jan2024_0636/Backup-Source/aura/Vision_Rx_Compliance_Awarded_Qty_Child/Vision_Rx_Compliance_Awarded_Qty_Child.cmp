                    </td>
                    
                    <aura:if isTrue="{!v.itemDescDrover}">
                        <td class="ndcColumnData fixedcolumn2" title="" style="max-width:100px !important;overflow: hidden;">{!singlerec.Vision_Product__r.Phoenix_NDC_11__c}</td>
                        
                        <td class="packColumnData fixedcolumn3" style="text-align:center;max-width:80px !important;overflow: hidden;"><lightning:formattedNumber value="{!singlerec.Vision_Product__r.Phoenix_Pkg_Size__c}" minimumFractionDigits="0" maximumFractionDigits="0"/></td>
