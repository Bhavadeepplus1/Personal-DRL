                            <td class="redIcon QTDColumnData" ><lightning:icon iconName="utility:dislike" title="{!singlerec.QTD_Total_Complaince__c }" size="x-small"/></td>
                            
                        </aura:set>
                    </aura:if> 
                    <aura:if isTrue="{!or(singlerec.FTD_Total_Compliance__c ge 75,singlerec.FTD_Total_Compliance__c == undefined )}">          
