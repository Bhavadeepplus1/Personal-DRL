                            <td class="redIcon month4ColumnData" ><lightning:icon iconName="utility:dislike" title="{!singlerec.M4_Total_Compliance__c }" size="x-small"/></td>
                            
                        </aura:set>
                    </aura:if> 
                    <aura:if isTrue="{!or(singlerec.M3_Total_Compliance__c ge 75,singlerec.M3_Total_Compliance__c == undefined )}">          
