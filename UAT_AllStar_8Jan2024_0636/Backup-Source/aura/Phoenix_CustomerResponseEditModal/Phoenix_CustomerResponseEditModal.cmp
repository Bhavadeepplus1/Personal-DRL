<aura:component controller="Phoenix_CustomerResponseViewController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <aura:attribute name="recordId" type="String" />
    <lightning:overlayLibrary aura:id="overlayLibDemo1"/>
    <lightning:notificationsLibrary aura:id="notifLib"/>
    
    <aura:attribute name="isFromEditButton" type="Boolean" default="false"/>
    <aura:attribute name="bidRecordId" type="String" default="false"/>
    <aura:attribute name="bidCustomerId" type="String" default="false"/>
    <aura:attribute name="bidType" type="String"/>
    <aura:attribute name="isSpinnerLoad" type="Boolean" default="false"/>
    <aura:attribute name="isBidLISubmitted" type="Boolean" default="false"/>
    <aura:attribute name="isVistexPending" type="Boolean" default="false"/>
    
    <aura:attribute name="isLoginUserAbleToPerform" type="boolean" default="false" />
    <aura:attribute name="isLoginUserAbleToPerformVU" type="boolean" default="false" />
    
    <aura:attribute name="contractListFromBid" type="String" default=''/>
    <aura:attribute name="contractVistexUpdate" type="String" default=''/>
    
    
    <!--Contract Model section-->
    <aura:attribute name="showModal" type="boolean" default="false"/>
    <aura:attribute type="List" name="contratcsList"/>
    <aura:attribute name="mycolumns" type="List"/>
    <aura:attribute name="selectedCntrcts"  type="list"/>    
    <aura:attribute name="isSpinner" type="Boolean" default="false"/>
    
    <!--code by Rama-->
    <aura:attribute name="CompetitorInfowrapper" type="object"  />
    <aura:attribute name="navigate" type="boolean" default="false" />
    <aura:attribute name="selectedCompetitor" type="String" />
    <aura:attribute name="Competitorlist" type="object"  />
    
    <aura:attribute name="incomes" type="Object"/>
    <aura:attribute name="ProductFamilyNames" type="object" />

    <aura:attribute name="selectedLookUpRecord" type="sObject" default="{}"/>
    
    <aura:handler name="init" value="this" action="{!c.doInit}"/>
    <aura:attribute name="userInfo" type="user"/> 
    
    <aura:attribute name="AccountName" type="object"/>
    
    <aura:attribute name="newrec" type="boolean" default="false"/>
    <aura:attribute name="ProductFamilyNamesNew" type="object" />
    <aura:attribute name="CompetitorInfowrapperNew" type="object" />

    <aura:handler name="init" value="{!this}" action="{!c.customerName}"/>
    
    <aura:html tag="style">
        .slds-modal__container{ 
        width: 100%;
        } 
        
    </aura:html>
    
    <!--code by Rama end -->
    
    <div class="slds-theme_default">
        <aura:if isTrue="{!v.isSpinnerLoad}">
            <div class="backgroundInverse">
                <lightning:spinner alternativeText="Loading" variant="brand" />
            </div>
        </aura:if>
        
        <aura:if isTrue="{!v.showModal}">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" 
                     aria-modal="true" aria-describedby="modal-content-id-1" 
                     class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close"
                                              onclick="{! c.hideModel }"
                                              alternativeText="close"
                                              variant="bare-inverse"
                                              class="slds-modal__close"/>
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                            Contracts</h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium slds-is-relative" id="modal-content-id-1">
                        <aura:if isTrue="{!v.isSpinner}">
                            <lightning:spinner size="medium" variant="brand" />
                        </aura:if>
                        <p>
                            <div class="slds-grid" style="margin-bottom:6px;">
                                <div class="slds-col slds-size_1-of-3">
                                    <div aura:id="" class="slds-combobox__form-element slds-input-has-icon slds-input-has-icon_right"  style="width: 100%;">
                                        <ui:inputText aura:id="cntInput" class="slds-lookup__search-input slds-input inputSize"
                                                      placeholder="Search contracts..."/>
                                        <span class="slds-icon_container slds-icon-utility-search slds-input__icon slds-input__icon_right">
                                            <lightning:icon iconName="utility:search" size="x-small" alternativeText="icon"/>
                                        </span>
                                    </div> 
                                </div>
                                <div class="slds-col slds-size_1-of-3">
                                    <div class="slds-grid">
                                        <div class="slds-col slds-size_1-of-2">
                                            <lightning:input class="slds-p-left_large slds-p-top_x-small" type="checkbox" label="Show All" name="input1" aura:id="tgleCntrct" onchange="{!c.searchContracts}"/>
                                        </div>
                                        <div class="slds-col slds-size_1-of-2">
                                            <div  class="slds-m-top_medium slds-m-left_small" style="margin-top:1px;"><lightning:button label="Search" onclick="{!c.searchContracts}"  variant="brand"/></div> 
                                        </div>
                                    </div>
                                </div>                                       
                            </div>  
                            <aura:if isTrue="{!not(empty(v.contratcsList))}">                            
                                <lightning:datatable aura:id="linesTable"
                                                     data="{! v.contratcsList }" 
                                                     columns="{! v.mycolumns }"
                                                     keyField="id"                                                
                                                     />
                                <aura:set attribute="else">
                                    <center>
                                        <div class="slds-notify slds-notify_alert slds-theme_alert-texture slds-theme_info slds-m-left_xx-large slds-m-right_xx-large" role="alert" style="width:50%;">
                                            <span class="slds-icon_container slds-m-right_x-small" title="Please add line items to view the table">
                                                <lightning:icon iconName="utility:info" alternativeText="info" size="x-small" variant="inverse"/>
                                            </span>
                                            <h2> No Records Found</h2>
                                        </div>
                                    </center>
                                </aura:set>
                            </aura:if>
                        </p>
                    </div>
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        <lightning:button variant="neutral"
                                          label="Cancel"
                                          title="Cancel"
                                          onclick="{! c.hideContractModel }"/>
                        <lightning:button variant="brand"
                                          label="Save"
                                          title="Save"
                                          disabled= "{!empty(v.contratcsList)}"
                                          onclick="{!c.saveDetails}"/>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
        
        <lightning:recordEditForm aura:id="CRForm"
                                  recordId="{!v.recordId}"
                                  objectApiName="Phoenix_Customer_Response__c"     
                                  onload="{!c.handleOnload}"
                                  onsuccess="{!c.handleOnSuccess}"
                                  onsubmit="{!c.onRecordSubmit}" 
                                  class="slds-card__body_inner">
            <lightning:messages/>
            <h3 class="slds-section-title--divider slds-m-vertical_small">Customer Response Information</h3>
            <div class="">
                <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <lightning:outputField fieldName="Phoenix_Bid_No__c"/>
                    </div>
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <lightning:inputField fieldName="Phoenix_Bid_Name__c" disabled="{!v.isBidLISubmitted}" />
                    </div>      
                </div>
                <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
						<lightning:outputField fieldName="Phoenix_Customer__c"/>
                    </div>   
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <aura:if isTrue="{!v.isBidLISubmitted}">
                            <lightning:outputField fieldName="Phoenix_Sent_to_Customer_Date__c"/>
                            <aura:set attribute="else">
                                <lightning:inputField fieldName="Phoenix_Sent_to_Customer_Date__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                            </aura:set>
                        </aura:if>
                    </div>   
                </div>
                <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <aura:if isTrue="{!v.isBidLISubmitted}">
                            <lightning:outputField fieldName="Phoenix_Customer_Response_Date__c"/>
                            <aura:set attribute="else">
                                <lightning:inputField fieldName="Phoenix_Customer_Response_Date__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                            </aura:set>
                        </aura:if>
                    </div>
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <aura:if isTrue="{!v.isBidLISubmitted}">
                            <lightning:outputField fieldName="Phoenix_Items_Pending__c" required="true"/>
                            <aura:set attribute="else">
                                <lightning:inputField fieldName="Phoenix_Items_Pending__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                            </aura:set>
                        </aura:if>
                    </div>                                     
                </div>
            </div>
            <h3 class="slds-section-title--divider slds-m-vertical_small">Change Type Update</h3>
            <div class="">
                <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <div class="slds-grid">
                            <label class="slds-col slds-form-element__label slds-p-around_xx-small slds-size_1-of-3" for="pricingid">Pricing</label>
                            <aura:if isTrue="{!v.isBidLISubmitted}">
                                <lightning:outputField  class="slds-col slds-p-around_xx-small slds-p-left_none slds-size_2-of-3" aura:id="pricingid" variant="label-hidden" fieldName="Phoenix_Change_Type_Pricing__c"/>
                                <aura:set attribute="else">
                                    <lightning:inputField  class="slds-col slds-p-around_xx-small slds-p-left_none slds-size_2-of-3" aura:id="pricingid" variant="label-hidden" fieldName="Phoenix_Change_Type_Pricing__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                                </aura:set>
                            </aura:if>
                        </div>
                    </div>
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <div class="slds-grid">
                            <label class="slds-col slds-form-element__label slds-p-around_xx-small slds-size_1-of-3" for="rebateid">Rebate</label>
                            <aura:if isTrue="{!v.isBidLISubmitted}">
                                <lightning:outputField  class="slds-col slds-p-around_xx-small slds-p-left_none slds-size_2-of-3" aura:id="rebateid" variant="label-hidden" fieldName="Phoenix_Change_Type_Rebate__c"/>
                                <aura:set attribute="else">
                                    <lightning:inputField  class="slds-col slds-p-around_xx-small slds-p-left_none slds-size_2-of-3" aura:id="rebateid" variant="label-hidden" fieldName="Phoenix_Change_Type_Rebate__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                                </aura:set>
                            </aura:if>
                        </div>
                    </div>
                </div>
                <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <aura:if isTrue="{!!v.isBidLISubmitted}">
                            <div class="slds-form-element slds-lookup slds-is-close frequired">
                                <label class="slds-form-element__label ">Contract #'s for Position/Volume</label>
                                <div class="slds-form-element__control">
                                    <div class="slds-input-has-icon slds-input-has-icon--right">
                                        <div aura:id="" class="slds-pill-container">
                                            <div aura:id="" class="slds-combobox__form-element slds-input-has-icon slds-input-has-icon_right"  style="width: 100%;">
                                                <ui:inputText disabled="{!not(v.isLoginUserAbleToPerform)}"
                                                              click="{!c.showModel}" 
                                                              updateOn="keyup"
                                                              class="slds-lookup__search-input slds-input inputSize"
                                                              placeholder="Click to select contracts here.."/>
                                                <span class="slds-icon_container slds-icon-utility-search slds-input__icon slds-input__icon_right">
                                                    <lightning:icon iconName="utility:search" size="x-small" alternativeText="icon"/>
                                                </span>
                                            </div> 
                                            <aura:if isTrue="{!not(v.isLoginUserAbleToPerform)}">
                                                <aura:iteration items="{!v.selectedCntrcts}" var="sr">
                                                    <lightning:pill class="slds-m-around_xx-small pillRemove" 
                                                                    label="{!sr}" 
                                                                    name="{!sr}" 
                                                                    onremove="{! c.clearCntract }">
                                                    </lightning:pill>
                                                </aura:iteration>
                                                <aura:set attribute="else">
                                                    <aura:iteration items="{!v.selectedCntrcts}" var="sr">
                                                        <lightning:pill class="slds-m-around_xx-small" 
                                                                        label="{!sr}" 
                                                                        name="{!sr}" 
                                                                        onremove="{! c.clearCntract }">
                                                        </lightning:pill>
                                                    </aura:iteration>
                                                </aura:set>
                                            </aura:if>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <aura:set attribute="else">
                                <lightning:outputField fieldName="Phoenix_Affected_Contract_s__c"/>
                            </aura:set>
                        </aura:if>
                    </div>
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2" >
                        <aura:if isTrue="{!v.isBidLISubmitted}">
                            <lightning:outputField fieldName="Phoenix_Contracts_for_Vistex_Update__c"/>
                            <aura:set attribute="else">
                                <lightning:inputField fieldName="Phoenix_Contracts_for_Vistex_Update__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                            </aura:set>
                        </aura:if>
                    </div>
                </div>
                <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <aura:if isTrue="{!v.isBidLISubmitted}">
                            <lightning:outputField fieldName="Phoenix_Special_Instructions_to_AGR__c"/>
                            <aura:set attribute="else">
                                <lightning:inputField fieldName="Phoenix_Special_Instructions_to_AGR__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                            </aura:set>
                        </aura:if>
                    </div>
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <aura:if isTrue="{!v.isBidLISubmitted}">
                            <lightning:outputField fieldName="Delay_Type__c"  required="true"/>
                            <aura:set attribute="else">
                                <lightning:inputField fieldName="Delay_Type__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                            </aura:set>
                        </aura:if>
                    </div>
                </div>
                 <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <aura:if isTrue="{!v.isBidLISubmitted}">
                            <lightning:outputField fieldName="Ratification_Comments__c"/>
                            <aura:set attribute="else">
                                <lightning:inputField fieldName="Ratification_Comments__c" disabled="{!not(v.isLoginUserAbleToPerform)}"/>
                            </aura:set>
                        </aura:if>
                    </div>
                  
                </div>
            </div>
            
            <aura:if isTrue="{!not(v.bidType == 'Volume Review Only')}">
                <h3 class="slds-section-title--divider slds-m-vertical_small">Contract Status</h3>
                <div class="">
                <div class="slds-grid">
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                        <lightning:outputField fieldName="Phoenix_Contract_Status__c"/>
                    </div>
                    <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
						  <lightning:outputField fieldName="Phoenix_Contract_Update_Date__c"/>
                    </div>
                </div>
            </div>
            </aura:if>
            <aura:if isTrue="{!not(v.bidType == 'Volume Review Only')}">
                <h3 class="slds-section-title--divider slds-m-vertical_small">Vistex Status</h3>
                <div class="">
                    <div class="slds-grid">
                        <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                            <aura:if isTrue="{!v.isBidLISubmitted == false}">
                                <lightning:outputField fieldName="Phoenix_Vistex_Status__c"/>
                                <aura:set attribute="else">
                                    <lightning:inputField aura:id="vistexId" fieldName="Phoenix_Vistex_Status__c" disabled="{!or(v.isVistexPending == false, not(v.isLoginUserAbleToPerformVU))}"/>
                                </aura:set>
                            </aura:if>
                        </div>
                        <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                            <aura:if isTrue="{!v.isBidLISubmitted == false}">
                                <lightning:outputField fieldName="Phoenix_Vistex_Comments__c"/>
                                <aura:set attribute="else">
                                    <lightning:inputField fieldName="Phoenix_Vistex_Comments__c" disabled="{!or(v.isVistexPending == false, not(v.isLoginUserAbleToPerformVU))}"/>
                                </aura:set>
                            </aura:if>
                        </div>
                    </div>
					<div class="slds-grid">
                        <div class="slds-col slds-p-around_xx-small slds-size_1-of-2">
                            <lightning:outputField fieldName="Phoenix_Vistex_Update_Date__c"/>
                        </div>
                    </div>
                    <!--code by Rama-->
                    <aura:if isTrue="{!and(v.userInfo.Name != 'Surender Patel',v.userInfo.Name != 'Sneha Shah',v.userInfo.Name != 'Sudheer Nanduri',v.userInfo.Name != 'Leigh Avery',v.userInfo.Name != 'Matthew Bilbao',v.userInfo.Name != 'Matthew Bilbao',v.userInfo.Name != 'Abhijit Patil',v.userInfo.Name != 'Ahmed Rifayee',v.userInfo.Name != 'Shruti Shah',v.userInfo.Name != 'Saunak Savla')}">
                        <lightning:button variant="neutral" label="Winning Competitor Info" title="Competitor Info" onclick="{!c.WinningCompetitorInfobuttion}" />
                    </aura:if>
                    <aura:if isTrue="{!v.navigate}">
                        
                        <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_large" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1">
                            <div class="slds-modal__container"  style="max-width: 90rem !important;width: 90% !important;">
                                <header class="slds-modal__header">                                   
                                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">Winning Competitor Info</h2>
                                    <div class="slds-form_horizontal" style="width:18%">
                                        <label for="CustomerId">Customer Name: {!v.AccountName.Phoenix_Customer__r.Name}</label><br/>
                                        <lightning:outputField  fieldName="" aura:id="customername1" variant="label-hidden"/>
                                    </div><br/>
                                    <lightning:layout>                              
                                        <lightning:layoutItem size="2" padding="around-small">                          
                                            <div class="slds-form_horizontal" style="width:365px">
                                                <lightning:select name="ChooseProductFamily" label="Product Family" aura:id="ChooseProductFamilyid" onchange="{!c.onChange }">
                                                    <option value="ChooseProductFamily">Choose Product Family</option>
                                                    <aura:iteration items="{!v.ProductFamilyNames}" var="color">
                                                        <option 	text="{!color.Product_Family__c}" value="{!color.Product_Family__c}" />
                                                    </aura:iteration>
                                                </lightning:select>
                                            </div>                              
                                        </lightning:layoutItem>                           
                                    </lightning:layout>
                                    <div class="slds-form_horizontal" style="width:36%;font-style: italic;">
                                        <label for="declm">(Price Range accepts only numbers and special characters e.g: +10%, 52.5 - 56.5)</label><br/>                         
                                    </div>
                                </header>
                                <div class="slds-modal__content slds-p-around_medium slds-scrollable" id="modal-content-id-1">
                                   <div class="" style="height: 100%; width:272rem;"> 
                                    <table class="slds-table slds-table_cell-buffer slds-table_bordered slds-table_striped" aria-label="Example table of Opportunities with striped rows">
                                        <thead>
                                            <tr class="slds-line-height_reset">                                               
                                                <th class="" scope="col" style="360px">
                                                    <div class="slds-truncate" title="Product Name">Product Name</div>
                                                </th>
                                                <th class="" scope="col" style="width:130px">
                                                    <div class="slds-truncate" title="NDC">NDC</div>
                                                </th>
                                                <th class="" scope="col" style="width:180px">
                                                    <div class="slds-truncate" title="Product Family">Product Family</div>
                                                </th>
                                                <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Winning Competitor Name">Winning Competitor Name</div>
                                                </th>
                                                <th class="" scope="col" style="width:60px">
                                                    <div class="slds-truncate" title="Winning Price Range">Winning Price Range</div>
                                                </th>
                                                <!-- Winning Competitor Info -->
                                                <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Winning Competitor Name 2">Winning Competitor Name 2</div>
                                                </th>
                                                <th class="" scope="col" style="width:60px">
                                                    <div class="slds-truncate" title="Winning Price Range 2">Winning Price Range 2</div>
                                                </th>
                                                <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Winning Competitor Name 3">Winning Competitor Name 3</div>
                                                </th>
                                                <th class="" scope="col" style="width:60px">
                                                    <div class="slds-truncate" title="Winning Price Range 3">Winning Price Range 3</div>
                                                </th>
                                                <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Winning Competitor Name 4">Winning Competitor Name 4</div>
                                                </th>
                                                <th class="" scope="col" style="width:60px">
                                                    <div class="slds-truncate" title="Winning Price Range 4">Winning Price Range 4</div>
                                                </th>
                                                 <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Winning Competitor Name 5">Winning Competitor Name 5</div>
                                                </th>
                                                <th class="" scope="col" style="width:60px">
                                                    <div class="slds-truncate" title="Winning Price Range 5">Winning Price Range 5</div>
                                                </th>
                                                <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Incumbent Competitor Name">Incumbent Competitor Name</div>
                                                </th>
                                                <th class="" scope="col" style="width:60px">
                                                    <div class="slds-truncate" title="Incumbent Price Range">Incumbent Price Range</div>
                                                </th>
                                                 <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Incumbent Competitor Name 2">Incumbent Competitor Name 2</div>
                                                </th>
                                                <th class="" scope="col" style="width:60px">
                                                    <div class="slds-truncate" title="Incumbent Price Range 2">Incumbent Price Range 2</div>
                                                </th>
                                                 <th class="" scope="col" style="width:198px">
                                                    <div class="slds-truncate" title="Incumbent Competitor Name 3">Incumbent Competitor Name 3</div>
