                        <div class="slds-backdrop slds-backdrop_open"></div>
                    </aura:if>
                    <!--code by Rama end-->
                </div>
            </aura:if>
            <center class="slds-m-vertical_medium">
                <aura:if isTrue="{!not(and(v.isFromEditButton == false, and(v.isVistexPending == false, v.isBidLISubmitted)))}">
                    <lightning:button label="Cancel" onclick="{!c.doCancel}" variant="neutral" disabled="{!or(v.isFromEditButton,and(v.isBidLISubmitted == false, not(v.isLoginUserAbleToPerform)), and(v.isBidLISubmitted,not(v.isLoginUserAbleToPerformVU)))}"/>&nbsp; &nbsp;
                    <lightning:button variant="brand" label="Save" type="submit" disabled="{!or(and(not(v.isBidLISubmitted),not(v.isLoginUserAbleToPerform)), and(v.isVistexPending == false, v.isBidLISubmitted, not(v.isLoginUserAbleToPerformVU)))}"/> 
                </aura:if>
            </center>
        </lightning:recordEditForm>
    </div>
</aura:component>