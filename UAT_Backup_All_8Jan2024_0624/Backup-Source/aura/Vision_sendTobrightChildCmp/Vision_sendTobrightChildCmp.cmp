        <td class="slds-text-align--center">
            <aura:if isTrue="{!!v.isSentToBright}">
                <lightning:icon class="changeDeleteIconColor"
                                iconName="utility:delete"
                                onclick="{!c.removeDeletedRow}"
                                name="{!v.rowIndex}" size="x-small"
