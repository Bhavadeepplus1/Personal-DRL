    </tr>
      <aura:if isTrue="{!v.addCust}">
        <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_medium" aria-labelledby="modal-heading-01" aria-modal="true">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{! c.closeModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close buttonCSS"/>
                    <div style="font-size:1px;"><b>Secondary Exclusion Customers</b></div>
                </header>
                <div class="slds-modal__content slds-p-around_medium slds-is-relative" id="modal-content-id-1">
                    <div class="slds-scrollable">
                        <table aura:id="LineTable" class="slds-table slds-no-row-hover slds-table_bordered slds-table_col-bordered slds-table--header-fixed">
                            <thead>
                                <tr>
                                    <th style="padding:10px;font-weight:Bold;text-align:left;background-color:#257CFF;color:white;">Is Exclusion Customer?</th>
                                    <th style="padding:10px;font-weight:Bold;text-align:left;background-color:#257CFF;color:white;">Customer Name</th>
                                    <th style="padding:10px;font-weight:Bold;text-align:left;background-color:#257CFF;color:white;">Contract Number</th>
                                    <th style="padding:10px;font-weight:Bold;text-align:left;background-color:#257CFF;color:white;">12 Months Sales Unit</th>
                                    <th style="padding:10px;font-weight:Bold;text-align:left;background-color:#257CFF;color:white;">Contract Price</th>
                                    <th style="font-weight:Bold;text-align:center;background-color:#257CFF;color:white;">Product Position</th> 
                                </tr>
                            </thead>
                            <tbody>
                                <aura:iteration items="{!v.secondaryList}" var="item" >
                                    <tr> <aura:if isTrue="{!item.isSelected}">
                                        <td><lightning:input type="checkbox" checked="{!item.isSelected}" disabled="{!v.item.isDisabled}"/></td>
                                        <aura:set attribute="else">
                                       <td><lightning:input type="checkbox" checked="{!item.isSelected}" /></td> 
                                        </aura:set>
                                        </aura:if>
                                        <td>{!item.custName}</td>
                                        <td>{!item.contractNumber}</td>
                                        <td style="text-align:right"> <lightning:formattedNumber value="{!item.units}" minimumFractionDigits= "0" maximumFractionDigits= "0"/></td>
                                        <td style="text-align:right"> <lightning:formattedNumber value="{!item.contractPrice}" style="currency" currencyCode="USD" minimumFractionDigits= "0"/></td>
                                        <td>{!item.position}</td>
                                        
                                    </tr>
                                </aura:iteration>
                            </tbody>
                        </table>  
                        </div>  <div style="padding:10px;text-align:right;" >
                            <lightning:button label="Move" variant="brand" onclick="{! c.showConfirmationPopup }"/>
                            <lightning:button label="Close" onclick="{! c.closeModel }"/>
                            
                        </div>
                  
                </div>
            </div>
            
        </section>
        <div class="slds-backdrop slds-backdrop_open" role="presentation"></div>
    </aura:if> 
        <aura:if isTrue="{!v.showConfirmation}">
                    <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" 
                             aria-modal="true" aria-describedby="modal-content-id-1" 
                             class="slds-modal slds-fade-in-open slds-modal_medium">
                        <div class="slds-modal__container">
                            <header class="slds-modal__header">
                                <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                                    </h2>
                            </header>
                            <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                                <p><div style="text-align:center;padding-bottom:3px;">  Are you sure you want move the customer to final exclusion customer list?</div>
                                </p>
                            </div>
                            <!--Modal/Popup Box Footer Starts here-->
                            <footer class="slds-modal__footer">
                                <lightning:button variant="neutral"
                                                  label="No"
                                                  title="No"
                                                  onclick="{! c.closeModelConfirmation }"/>
                                <lightning:button variant="brand"
                                                  label="Yes"
                                                  title="Yes"
                                                  onclick="{!c.saveCustomer}"/>
                            </footer>
                        </div>
                    </section>
                    <div class="slds-backdrop slds-backdrop_open"></div>
                </aura:if>
    
</aura:component>