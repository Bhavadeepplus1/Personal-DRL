                    <h2  class="slds-modal__title slds-hyphenate" style="color:#675e5e">
                        <p style="font-size:21px;"><b>Products for WAC Change</b></p>   
                    </h2>
                </div>
                <aura:if isTrue="{!v.showNoRecMsg}">
