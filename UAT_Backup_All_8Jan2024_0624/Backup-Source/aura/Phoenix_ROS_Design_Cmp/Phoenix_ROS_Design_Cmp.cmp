                                            <div class="slds-col slds-size_1-of-4">
                                                <lightning:inputField aura:id="bidCustmr" fieldName="Phoenix_Customer__c" disabled="true" />
                                            </div>
                                            <div class="slds-col slds-size_1-of-4">
                                                <lightning:inputField fieldName="Phoenix_Proposed_Admin_Fee_Basis__c" onchange="{!c.showSaveCancel}" disabled="true"/>
                                            </div>
                                            <div class="slds-col slds-size_1-of-4">
                                                <lightning:select name="Initial Order Discount Basis" 
