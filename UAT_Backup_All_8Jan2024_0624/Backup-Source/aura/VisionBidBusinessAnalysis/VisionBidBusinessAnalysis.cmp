<aura:component controller="VisionBidBusinessAnalysisCls" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <aura:attribute name="options" type="List" default="[
                                                        {'label': 'Rx', 'value': 'Rx'},
                                                        {'label': 'SRx', 'value': 'SRx'},
                                                        {'label': 'OTC', 'value': 'OTC'},
                                                        {'label': 'All', 'value': 'All'},
                                                        ]"/>
    <aura:attribute name="productOptions" type="List" default="[
                                                               {'label': 'Rx', 'value': 'Rx'},
                                                               {'label': 'SRx', 'value': 'SRx'},
                                                               {'label': 'OTC', 'value': 'OTC'},
                                                               {'label': 'Rx+SRx', 'value': 'Rx+SRx'},
                                                               {'label': 'SRx+OTC', 'value': 'SRx+OTC'},
                                                               {'label': 'Rx+OTC', 'value': 'Rx+OTC'},
                                                               {'label': 'All', 'value': 'All'},
                                                               ]"/>
    <aura:attribute name="dateOptions" type="list" default="[
                                                            {'label': 'Customer Response Date', 'value': 'Customer Response Date'},
                                                            {'label': 'Sent to Customer Date', 'value': 'Sent to Customer Date'},
                                                            {'label': 'Pending (Internal)', 'value': 'Pending'},
                                                            {'label': 'Internal Rejections', 'value': 'Internal Rejections'},
                                                            ]" />
    <aura:attribute name="directorType" type="list" default="[
                                                             {'label': 'Account Director', 'value': 'Account Director'},
                                                             {'label': 'Salesperson', 'value': 'Salesperson'},
                                                             {'label': 'Product Director', 'value': 'Product Director'},
                                                             ]" />
    <aura:attribute name="selections" type="list" />
    <aura:attribute name="activeSections" type="List" />
    <aura:attribute name="selectedId" type="string" />
    <aura:attribute name="selectedUserId" type="string" />
    <aura:attribute name="selectedDivision" type="string" default="All" />
    <aura:attribute name="selectedDate" type="string" default="Sent to Customer Date" />
    <aura:attribute name="selectedDirectorType" type="string" default="Account Director" />
    <aura:attribute name="isRxChecked" type="boolean" default="true" />
    <aura:attribute name="isSRxChecked" type="boolean" default="true" />
    <aura:attribute name="isOtcChecked" type="boolean" default="true"/>
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="scmData" type="List" />
    <aura:attribute name="onLoad" type="boolean" default="true" />
    <aura:attribute name="openStatusList" type="list" />
    <aura:attribute name="gainedList" type="List" />
    <aura:attribute name="gainedOTBList" type="List" />
    <aura:attribute name="retainedGainList" type="List" />
    <aura:attribute name="internalRejectionList" type="List" />
    <aura:attribute name="retainedLossList" type="List" />
    <aura:attribute name="lossList" type="List" />
    <aura:attribute name="RFPLossList" type="List" />
    <aura:attribute name="PCLossList" type="List" />
    <aura:attribute name="lossDataRemovals" type="Object" />
    <aura:attribute name="lossDataDiscontinuation" type="Object" />
    <aura:attribute name="lossDataRFP" type="Object" />
    <aura:attribute name="removalsLostLabel" type="string" />
    <aura:attribute name="discontinuationLostLabel" type="string" />
    <aura:attribute name="RFPLostLabel" type="string" />
    <aura:attribute name="PCLostLabel" type="string" />
    <aura:attribute name="internalRejectionLabel" type="string" />
    <aura:attribute name="noEffectList" type="List" />
    <aura:attribute name="categoryTotals" type="Object" />
    <aura:attribute name="bidNumber" type="string"/>
    <aura:attribute name="templateType" type="string"/>
    <aura:attribute name="totalBids" type="Integer" />
    <aura:attribute name="totalLineItems" type="Integer" />
    <aura:attribute name="bidName" type="string"  />
    <aura:attribute name="title" type="String" />
    <aura:attribute name="isSpinnerLoad" type="boolean" default="false" />
    <aura:attribute name="totalAnnualImpact" type="integer" />
    <aura:attribute name="totalBusinessImpact" type="integer" />
    <aura:attribute name="isModalOpen" type="boolean" default="false"/>
    <aura:attribute name="showAddProducts" type="boolean" default="true"/>
    <aura:attribute name="showWaterfallChart" type="boolean" default="false" />
    <aura:attribute name="startDate" type="string" />  <!-- YYYY-MM-DD -->
    <aura:attribute name="endDate" type="string" />
    <aura:attribute name="businessGained" type="string"/>
    <aura:attribute name="otb" type="string"/>
    <aura:attribute name="retainedGain" type="string"/>
    <aura:attribute name="retainedLoss" type="string"/>
    <aura:attribute name="openStatus" type="string"/>
    <aura:attribute name="noEffect" type="string"/>
    <aura:attribute name="lost" type="string"/>
    <aura:attribute name="selectedFamily" type="String" />
    <aura:attribute name="familiesList" type="list" />
    <aura:attribute name="actualSales" type="integer" />
    <aura:attribute name="showFamilies" type="boolean" />
    <aura:attribute name="showProducts" type="boolean" />
    <aura:attribute name="openLineItemsCount" type="Integer" />
    <aura:attribute name="showLegendPopup" type="boolean" default="false" />
    <aura:attribute name="duration" type="String" />
    
    <aura:attribute name="totalMap" type="Object" />
    <aura:attribute name="totalFamilySummaryMap" type="Object" />
    <aura:attribute name="gainedMap" type="Object" />
    <aura:attribute name="gainedFamilySummaryMap" type="Object" />
    <aura:attribute name="gainedOTBMap" type="Object" />
    <aura:attribute name="gainedOTBFamilySummaryMap" type="Object" />
    <aura:attribute name="retainedGainMap" type="Object" />
    <aura:attribute name="retainedGainFamilySummaryMap" type="Object" />
    <aura:attribute name="internalRejectionMap" type="Object" />
    <aura:attribute name="internalRejectionFamilySummaryMap" type="Object" />
    <aura:attribute name="retainedLossMap" type="Object" />
    <aura:attribute name="retainedLossFamilySummaryMap" type="Object" />
    <aura:attribute name="RFPLossMap" type="Object" />
    <aura:attribute name="RFPLossFamilySummaryMap" type="Object" />
    <aura:attribute name="PCLossMap" type="Object" />
    <aura:attribute name="PCLossFamilySummaryMap" type="Object" />
    <aura:attribute name="noEffectMap" type="Object" />
    <aura:attribute name="noEffectFamilySummaryMap" type="Object" />
    <aura:attribute name="openStatusMap" type="Object" />
    <aura:attribute name="openStatusFamilySummaryMap" type="Object" />
    <aura:attribute name="openStatusBidTypeMap" type="Object" />
    <aura:attribute name="openStatusBidTypeSummaryMap" type="Object" />
    <aura:attribute name="selectedBidIds" type="list" default="[]"/>
    <aura:attribute name="currentTabId" type="string" default='tabOne' />
    
    <aura:attribute name="showOpenStatusDropDown" type="boolean" default="false"/>
    <aura:attribute name="showPriceChangeDropDown" type="boolean" default="false"/>
    <aura:attribute name="showVolumeChangeDropDown" type="boolean" default="false" />
    <aura:attribute name="showBothChangeDropDown" type="boolean" default="false" />
    <aura:attribute name="showLostDropDown" type="boolean" default="false" />
    <aura:attribute name="dynamicMargin" type="Decimal" default="192" />
    <aura:attribute name="openStatusSummaryData" type="list" />
    <aura:attribute name="bidTypes" type="list" />
    <aura:attribute name="expandAll" type="boolean" />
    
    <!-- attributes to be received from parent component-->
    <aura:attribute name="label" type="String" />
    <aura:attribute name="objectApiName" type="String" />
    <aura:attribute name="idFieldApiName" type="String" />
    <aura:attribute name="valueFieldApiName" type="String" />
    <aura:attribute name="extendedWhereClause" type="String" />
    <aura:attribute name="maxRecords" type="Integer" default="10"/>
    <aura:handler name="change" value="{!v.selectedBidIds}" action="{!c.selectedBidIdsChange}"/>
    
    
    <!-- Attributes specific to component-->
    <aura:attribute name="results" type="List" />
    <aura:attribute name="openDropDown" type="Boolean" default="false" />
    <aura:attribute name="selectedOption" type="String" />
    <aura:attribute name="inputValue" type="String" />
    <aura:attribute name="inputSearchFunction" type="Object" />
    
    <aura:method name="getSelectedBids" action="{!c.searchBidData}" access="public">
        <aura:attribute name="selectedBids" type="list" />
    </aura:method>
    
    <aura:handler name = "init" value ="{!this}" action = "{!c.doInit}"/>
    
    <div class="slds-box" style="background-color:white;">
        <div style="text-align: left;font-size: 20px;margin-bottom: 10px;">
            <b>Bid Business Analysis</b>
        </div>
        <lightning:tabset selectedTabId="one">
            <lightning:tab label="Analysis by filters" id="one" onactive="{!c.activeTabOne}">
                <div class="slds-grid">
                    <div class="slds-grid slds-col slds-size_8-of-12" style="border: 1px solid #cdcdcd;border-radius: 6px;padding-bottom: 4px;background-color: #d0e7f7;">
                        <div class=" slds-col slds-size_1-of-8" style="padding: 4px;">
                            <lightning:combobox name="Products" label="Products" value="All" placeholder="Products" options="{! v.productOptions }" onchange="{! c.searchSrxRxOtc }"/>    
                        </div>
                        <div class=" slds-col slds-size_1-of-8" style="padding: 4px 4px 4px 0px;">
                            <lightning:combobox name="division" label="Division" value="All" placeholder="Division" options="{! v.options }" onchange="{! c.handleChange }"/>
                        </div>
                        <div class="slds-grid slds-col slds-size_6-of-8" style="padding: 4px 4px 4px 0px;">
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <lightning:combobox name="directorType" label="Director/Initiator/PD" value="Account Director" placeholder="Type" options="{! v.directorType }" onchange="{! c.handleDirectorChange }"/>
                            </div>
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <aura:if isTrue="{!v.selectedDivision == 'All'}">
                                    <l_lookup:Lookup aura:id="accountDirectorLookup" 
                                                     label="{!v.selectedDirectorType}"
                                                     objectType="User"
                                                     placeHolder="{!v.selectedDirectorType}"
                                                     selectedRecordId="{!v.selectedId}" 
                                                     queryCondition=""
                                                     orderBy="Name"  />
                                    <aura:set attribute="else">
                                        <l_lookup:Lookup aura:id="accountDirectorLookup" 
                                                         label="{!v.selectedDirectorType}"
                                                         objectType="User"
                                                         placeHolder="{!v.selectedDirectorType}"
                                                         selectedRecordId="{!v.selectedId}" 
                                                         queryCondition="{! 'Division =\'' + v.selectedDivision + '\'' }"
                                                         dropDownLimit="3"
                                                         orderBy="Name"  />
                                    </aura:set>
                                </aura:if>
                            </div>
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <aura:if isTrue="{!v.selectedDirectorType == 'Account Director'}">
                                    <l_lookup:Lookup aura:id="directorRelatedAccountsLookup" 
                                                     label="Customer Account"
                                                     objectType="Account"
                                                     readOnly="{!or(v.selectedId == null, v.selectedId == '')}"
                                                     placeHolder="Search Customer Account"
                                                     selectedRecordId="{!v.selectedUserId}" 
                                                     queryCondition="{! 'OwnerId=\'' + v.selectedId + '\'' }"
                                                     dropDownQuery="{! 'OwnerId=\'' + v.selectedId + '\'' }"
                                                     orderBy="Name"  />
                                    <aura:set attribute="else">
                                        <l_lookup:Lookup aura:id="directorRelatedAccountsLookup" 
                                                         label="Customer Account"
                                                         objectType="Account"
                                                         readOnly="{!or(v.selectedId == null, v.selectedId == '')}"
                                                         placeHolder="Search Customer Account"
                                                         selectedRecordId="{!v.selectedUserId}"
                                                         orderBy="Name"  />
                                    </aura:set>
                                </aura:if>
                            </div>
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <!--<l_lookup:Lookup aura:id="productfamilies" 
                                                 label="Product Family"
                                                 objectType="Phoenix_Bid_Line_Product_Family__c"
                                                 placeHolder="Search Product Family"
                                                 selectedRecordId="{!v.selectedProductFamily}" 
                                                 queryCondition="{! 'Name=\'' + v.selectedProductFamily + '\'' }"
                                                 dropDownQuery="{! 'Name=\'' + v.selectedProductFamily + '\'' }"
                                                 orderBy="Name"  />-->
                                
                                <div class="slds-form-element">
                                    <label class="slds-form-element__label" for="combobox-id-21" >Product Family</label>
                                    <div class="slds-form-element__control">
                                        <div class="slds-combobox_container">
                                            <div class="{! v.openDropDown ? 'slds-combobox slds-dropdown-trigger slds-dropdown-trigger_click slds-is-open' : 'slds-combobox slds-dropdown-trigger slds-dropdown-trigger_click'}"
                                                 aria-expanded="true" aria-haspopup="listbox" role="combobox">
                                                <div class="slds-combobox__form-element slds-input-has-icon slds-input-has-icon_right" role="none">
                                                    <input type="text"
                                                           class="slds-input slds-combobox__input slds-combobox__input-value"
                                                           id="combobox-id-21" aria-autocomplete="list" aria-controls="listbox-id-11"
                                                           autoComplete="off" onkeyup="{!c.searchHandler}" role="textbox" placeholder="Product Family"
                                                           value="{!v.inputValue}" />
                                                    <aura:if isTrue="{! empty(v.selectedOption)}">
                                                        <span
                                                              class="slds-icon_container slds-icon-utility-search slds-input__icon slds-input__icon_right">
                                                            <lightning:icon iconName="utility:search" size="x-small" />
                                                        </span>
                                                        <aura:set attribute="else">
                                                            <button class="slds-button slds-button_icon slds-input__icon slds-input__icon_right"
                                                                    title="Clear the text input" onclick="{!c.clearOption}">
                                                                <lightning:icon iconName="utility:clear" size="x-small" />
                                                            </button>
                                                        </aura:set>
                                                    </aura:if>
                                                </div>
                                                <div id="listbox-id-11" class="slds-dropdown slds-dropdown_length-5 slds-dropdown_fluid"
                                                     role="listbox">
                                                    <ul class="slds-listbox slds-listbox_vertical" role="presentation">
                                                        <aura:iteration items="{!v.results}" var="result">
                                                            <li role="presentation" class="slds-listbox__item" data-id="{!result.id}"
                                                                data-value="{!result.value}" onclick="{!c.optionClickHandler}">
                                                                <div id="{!result.id}"
                                                                     class="slds-media slds-listbox__option slds-listbox__option_plain slds-media_small"
                                                                     role="option">
                                                                    <span class="slds-media__figure slds-listbox__option-icon"></span>
                                                                    <span class="slds-media__body">
                                                                        <span class="slds-truncate" title="All Families">
                                                                            <span>{!result.value}</span>
                                                                        </span>
                                                                    </span>
                                                                </div>
                                                            </li>
                                                        </aura:iteration>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="slds-grid slds-col slds-size_4-of-12" style="border: 1px solid #cdcdcd;border-radius: 6px;margin: 0px 4px;padding-bottom: 4px;background-color: #e3f2ed;">
                        <div class="slds-col slds-size_1-of-3" style="padding: 4px 4px 4px 4px;">
                            <lightning:combobox name="date" label="Based On" value="Sent to Customer Date" placeholder="Date Based on" options="{! v.dateOptions }" onchange="{! c.dateBasedOn }"/>
                        </div>
                        <div class="slds-col slds-size_1-of-3" style="padding: 4px 4px 4px 0px;">
                            <lightning:input aura:id="date1" type="date" name="date1" label="Start Date" dateStyle="medium" value="{!v.startDate}" fieldLevelHelp="{!v.selectedDate}"/>
                        </div>
                        <div class="slds-col slds-size_1-of-3" style="padding: 4px 4px 4px 0px;">
                            <lightning:input aura:id="date2" type="date" name="date2" label="End Date" dateStyle="medium" value="{!v.endDate}" fieldLevelHelp="{!v.selectedDate}"/>
                        </div>
                    </div>
                </div>
                <div style="margin: 8px;">
                    <div style="float: left;text-align: left;width: 33.3%;height: 31.6px;">
                        &nbsp;
                    </div>
                    <div style="float: left;text-align: center;width: 33.3%;height: 31.6px;">
                        <lightning:button variant="brand" label="Calculate" title="Calculate1" onclick="{!c.collectData }"/>
                    </div>
                    <div style="float: right;text-align: right;width: 33.3%;height: 31.6px;">
                        <aura:if isTrue="{!v.scmData.length == 0}">
                            &nbsp;
                            <aura:set attribute="else">
                                <aura:if isTrue="{!v.expandAll}">
                                    <lightning:button label="Collapse All" title="Collapse All" onclick="{! c.expandAll }"/>
                                    <aura:set attribute="else">
                                        <lightning:button label="Expand All" title="Expand All" onclick="{! c.expandAll }"/>
                                    </aura:set>
                                </aura:if>
                                <lightning:button label="Export" title="Export" onclick="{!c.downloadCategoryWiseCsv}"/>
                            </aura:set>
                        </aura:if>
                    </div>
                    <!--<span style="color:#5224B2; float: right;">Legends&nbsp;<lightning:icon class="purpleIcon" iconName="utility:in_app_assistant" size="small" onclick="{!c.openLegendPopup}"/></span>-->
                </div>
            </lightning:tab>
            <lightning:tab label="Search and Select Bids" id="two" onactive="{!c.refreshSearchTab}">
                <div style="text-align: right;margin: 4px;">
                    <lightning:button variant="neutral" label="Add Bids" onclick="{!c.refreshSearchTab}" />
                    <aura:if isTrue="{!v.scmData.length == 0}">
                        &nbsp;
                        <aura:set attribute="else">
                            <span style="padding: 0px 4px 0px 8px;">
                                <aura:if isTrue="{!v.expandAll}">
                                    <lightning:button label="Collapse All" title="Collapse All" onclick="{! c.expandAll }"/>
                                    <aura:set attribute="else">
                                        <lightning:button label="Expand All" title="Expand All" onclick="{! c.expandAll }"/>
                                    </aura:set>
                                </aura:if>
                                <lightning:button label="Export" title="Export" onclick="{!c.downloadCategoryWiseCsv}"/>
                            </span>
                        </aura:set>
                    </aura:if>
                </div>
                <!--<div style="text-align: center; margin: 8px 4px 4px 4px;">
                    <span style="color:#5224B2; float: right;">Legends&nbsp;<lightning:icon class="purpleIcon" iconName="utility:in_app_assistant" size="small" onclick="{!c.openLegendPopup}"/></span>
                </div>-->
                <c:VisionSearchSelectBids aura:id="searchTab" selectedBidIds = "selectedBidIds" showProducts="true" parent="{!this}"/>
            </lightning:tab>
            <lightning:tab label="Product to Customer Analysis" id="three" onactive="{!c.productToCustomer}">
                <div class="slds-grid">
                    <div class="slds-grid slds-col slds-size_8-of-12" style="border: 1px solid #cdcdcd;border-radius: 6px;padding-bottom: 4px;background-color: #d0e7f7;">
                        <div class=" slds-col slds-size_1-of-8" style="padding: 4px;">
                            <lightning:combobox name="Products" label="Products" value="All" placeholder="Products" options="{! v.productOptions }" onchange="{! c.searchSrxRxOtc }"/>    
                        </div>
                        <div class=" slds-col slds-size_1-of-8" style="padding: 4px 4px 4px 0px;">
                            <lightning:combobox name="division" label="Division" value="All" placeholder="Division" options="{! v.options }" onchange="{! c.handleChange }"/>
                        </div>
                        <div class="slds-grid slds-col slds-size_6-of-8" style="padding: 4px 4px 4px 0px;">
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <lightning:combobox name="directorType" label="Director/Initiator/PD" value="Account Director" placeholder="Type" options="{! v.directorType }" onchange="{! c.handleDirectorChange }"/>
                            </div>
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <aura:if isTrue="{!v.selectedDivision == 'All'}">
                                    <l_lookup:Lookup aura:id="accountDirectorLookup" 
                                                     label="{!v.selectedDirectorType}"
                                                     objectType="User"
                                                     placeHolder="{!v.selectedDirectorType}"
                                                     selectedRecordId="{!v.selectedId}" 
                                                     queryCondition=""
                                                     orderBy="Name"  />
                                    <aura:set attribute="else">
                                        <l_lookup:Lookup aura:id="accountDirectorLookup" 
                                                         label="{!v.selectedDirectorType}"
                                                         objectType="User"
                                                         placeHolder="{!v.selectedDirectorType}"
                                                         selectedRecordId="{!v.selectedId}" 
                                                         queryCondition="{! 'Division =\'' + v.selectedDivision + '\'' }"
                                                         dropDownLimit="3"
                                                         orderBy="Name"  />
                                    </aura:set>
                                </aura:if>
                            </div>
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <aura:if isTrue="{!v.selectedDirectorType == 'Account Director'}">
                                    <l_lookup:Lookup aura:id="directorRelatedAccountsLookup" 
                                                     label="Customer Account"
                                                     objectType="Account"
                                                     readOnly="{!or(v.selectedId == null, v.selectedId == '')}"
                                                     placeHolder="Search Customer Account"
                                                     selectedRecordId="{!v.selectedUserId}" 
                                                     queryCondition="{! 'OwnerId=\'' + v.selectedId + '\'' }"
                                                     dropDownQuery="{! 'OwnerId=\'' + v.selectedId + '\'' }"
                                                     orderBy="Name"  />
                                    <aura:set attribute="else">
                                        <l_lookup:Lookup aura:id="directorRelatedAccountsLookup" 
                                                         label="Customer Account"
                                                         objectType="Account"
                                                         readOnly="{!or(v.selectedId == null, v.selectedId == '')}"
                                                         placeHolder="Search Customer Account"
                                                         selectedRecordId="{!v.selectedUserId}"
                                                         orderBy="Name"  />
                                    </aura:set>
                                </aura:if>
                            </div>
                            <div class=" slds-col slds-size_1-of-4" style="padding: 0px 4px 4px 0px;">
                                <!--<l_lookup:Lookup aura:id="productfamilies" 
                                                 label="Product Family"
                                                 objectType="Phoenix_Bid_Line_Product_Family__c"
                                                 placeHolder="Search Product Family"
                                                 selectedRecordId="{!v.selectedProductFamily}" 
                                                 queryCondition="{! 'Name=\'' + v.selectedProductFamily + '\'' }"
                                                 dropDownQuery="{! 'Name=\'' + v.selectedProductFamily + '\'' }"
                                                 orderBy="Name"  />-->
                                
                                <div class="slds-form-element">
                                    <label class="slds-form-element__label" for="combobox-id-21" >Product Family</label>
                                    <div class="slds-form-element__control">
                                        <div class="slds-combobox_container">
                                            <div class="{! v.openDropDown ? 'slds-combobox slds-dropdown-trigger slds-dropdown-trigger_click slds-is-open' : 'slds-combobox slds-dropdown-trigger slds-dropdown-trigger_click'}"
                                                 aria-expanded="true" aria-haspopup="listbox" role="combobox">
                                                <div class="slds-combobox__form-element slds-input-has-icon slds-input-has-icon_right" role="none">
                                                    <input type="text"
                                                           class="slds-input slds-combobox__input slds-combobox__input-value"
                                                           id="combobox-id-21" aria-autocomplete="list" aria-controls="listbox-id-11"
                                                           autoComplete="off" onkeyup="{!c.searchHandler}" role="textbox" placeholder="Product Family"
                                                           value="{!v.inputValue}" />
                                                    <aura:if isTrue="{! empty(v.selectedOption)}">
                                                        <span
                                                              class="slds-icon_container slds-icon-utility-search slds-input__icon slds-input__icon_right">
                                                            <lightning:icon iconName="utility:search" size="x-small" />
                                                        </span>
                                                        <aura:set attribute="else">
                                                            <button class="slds-button slds-button_icon slds-input__icon slds-input__icon_right"
                                                                    title="Clear the text input" onclick="{!c.clearOption}">
                                                                <lightning:icon iconName="utility:clear" size="x-small" />
                                                            </button>
                                                        </aura:set>
