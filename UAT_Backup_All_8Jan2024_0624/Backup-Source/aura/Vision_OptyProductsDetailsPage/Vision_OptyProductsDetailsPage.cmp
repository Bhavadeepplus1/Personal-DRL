    <aura:attribute name="totalUnmatchdata" type="List" default="[]"/>
    
    <aura:attribute name="isUnMatchAsc" type="boolean"/>
    <aura:attribute name="matchcolumns" type="List"/>
    <aura:attribute name="unmatchcolumns" type="List"/>
    <aura:attribute name="selectedMatchRowsCount" type="Integer" default="0"/>
    <aura:attribute name="selectedUnmatchedRowsCount" type="Integer" default="0"/>
    <aura:attribute name="matchedRowsCount" type="Integer" default="0"/>
    <aura:attribute name="unmatchedRowsCount" type="Integer" default="0"/>
    <aura:attribute name="maxRowSelection" type="Integer"/>
    <aura:attribute name="selectedMatchedRowsList" type="List" />
    <aura:attribute name="selectedUnmatchedRowsList" type="List" />
    <aura:attribute name="isShowProducts" type="boolean" default="false" />
    <aura:attribute name="stageName" type="string" />
    <aura:attribute name="sentToPanorama" type="boolean" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:attribute name="isShowContracts" type="boolean" default="false" />
    <aura:attribute name="currentPage" type="integer" default="1"/>
    <aura:attribute name="isSpinnerLoad" type="Boolean" default="false"/>
    <aura:attribute name="templateType" type="String"/>
    <aura:attribute name="allContractsChecked" type="boolean" default='false'/>
    <aura:attribute name="selectedContId" type="list" />
    <aura:attribute name="PaginationList" type="list"/> 
    <aura:attribute name="isFirstPage" type="boolean" default="false"/>
    <aura:attribute name="isLastPage" type="boolean" default="false"/>
    <aura:attribute name="isSelectedNo" type="integer" default="1"/>
    <aura:attribute name="listOfAllAccounts" type="list"/> 
    <aura:attribute name="startPage" type="Integer" />
    <aura:attribute name="endPage" type="Integer"/>
    <aura:attribute name="totalRecordsCountCV" type="Integer"/>
    <aura:attribute name="pageSizeCV" type="Integer" default="100" description="number of records to be display on per page"/>
    <aura:attribute name="totalPagesCount" type="integer"/>
    <aura:attribute name="bNoRecordsFound" type="boolean"/>
    <aura:attribute name="searchValue" type="string" default=''/>
    <aura:attribute name="prdId" type="string"/>
    <aura:attribute name="prdName" type="string"/>
    <aura:attribute name="isRunOnce" type="boolean" default="true"/>
    <aura:attribute name="totalPages" type="Integer" default="0"/>
    <aura:attribute name="selectedRowsCount" type="Integer" default="0"/>
    <aura:attribute name="prdData" type="Object"/>
    <aura:handler name="onchange" event="l_lookup:OnChange" action="{!c.handleChange}"/>
    <aura:attribute name="selTabId" type="String" default="matchPrds"/>
    <!-- pagination attirbutes -->
    <aura:attribute name="totalNumberOfRecords" type="Integer"/>
    <aura:attribute name="pageNumberListMatched" type="List"/>
    <aura:attribute name="pageNumberListUnmatched" type="List"/>
    <aura:attribute name="selectedPageNumberMatched" type="Integer" default="1"/>
    <aura:attribute name="selectedPageNumberUnmatched" type="Integer" default="1"/>
    <aura:attribute name="pageNumberListDiscrepancy" type="List"/>
    <aura:attribute name="selectedPageNumberDiscrepancy" type="Integer" default="1"/>
    <aura:attribute name="selectedTab" type="String" default="matchPrds"/>
    <aura:attribute name="AccTemplate" type="string" />
    <!-- Product Info attirbutes -->
    <aura:attribute name="isShowPrdOptyInfo" type="boolean" default="false" />
    <aura:attribute name="infoTable" type="Object"/>
    <!-- Confirmation attirbutes -->
    <aura:attribute name="showConfirmation" type="boolean" default="false" />
    <aura:attribute name="selPrdId" type="string" />
    <aura:attribute name="confirmationMessage" type="string"/>
    <!-- discrepancy attirbutes -->
    <aura:attribute name="discdata" type="List" default="[]"/>
    <aura:attribute name="totalDiscdata" type="List" default="[]"/>
    <aura:attribute name="discRowsCount" type="Integer" default="0"/>
    <aura:attribute name="loadingMessage" type="String" default=""/>
    <!-- Direct/Indirect Template attirbutes -->
    <aura:attribute name="isShowDirectUnits" type="boolean" default="false" />
    <aura:attribute name="isShowInDirectUnits" type="boolean" default="false" />
    
    <aura:attribute name="noUsageList" type="List" default="[]"/>
    <aura:attribute name="duplicateProdList" type="List" default="[]"/>
    <aura:attribute name="otherDupProdList" type="List" default="[]"/>
    <aura:attribute name="inActiveList" type="List" default="[]"/>
    <aura:attribute name="inCorrectFormat" type="List" default="[]"/>
    <!--Attributes added by Naseer-->
    <aura:attribute name="allmatchdata" type="List" default="[]"/>
    <aura:attribute name="allunmatchdata" type="List" default="[]"/>
    <aura:attribute name="alldiscrepencydata" type="List" default="[]"/>
    <aura:attribute name="searchText" type="String" />
    
    <!-- Attributes added by Jogarao-->
    <aura:attribute name="filteredMatchData" type="List" default="[]"/>
    <aura:attribute name="filteredUnMatchData" type="List" default="[]"/>
    <aura:attribute name="filteredDescData" type="List" default="[]"/>
    <aura:attribute name="timer" type="Object"/>
    <aura:attribute name="isAsc" type="Boolean" default="false"/>
    <aura:attribute name="isDescAsc" type="Boolean" default="false"/>
    <aura:attribute name="isShowPrdOtyAnnualData" type="Boolean" default="false"/>
    <aura:attribute name="productsExclQury" type="String" />
    <aura:attribute name="existingProducts" type="List" default="[]"/>
    <aura:attribute name="bidonhighestpackList" type="List" default="[]"/>
    <aura:attribute name="bidonalterpackList" type="List" default="[]"/>
    <aura:attribute name="annualUnitsData" type="Object" />
    <aura:attribute name="selectedOptyProductId" type="String" />
    <aura:attribute name="selectedProId" type="String" />
    <aura:attribute name="refreshScreen" type="boolean" default="false"/>
    <aura:handler name="change" value="{!v.refreshScreen}" action="{!c.doInit}"/>
    <!-- attributes added by Sai Krishna on 21/03/2022 -->
    <aura:attribute type="String" name="sortField" />
    <!--<aura:attribute name="isAsc" type="boolean" default="true" description="boolean flag for pass sorting condition to apex class"/> 
    -->
    <aura:attribute name="sortAsc" type="boolean" default="true" />
    <aura:handler name="sampleCmpEvent" event="c:Vision_OptyProductsChildToParentEvnt" action="{!c.updateListOnChange}"/>
    <aura:attribute name="prodOptyIdList" type="List" default="[]"/>
    <aura:attribute name="productOptySelected" type="List" default="[]"/>
    <aura:attribute name="prodOptyMap" type="Map"/>
    <aura:handler name="visionPartialMatchEvent" event="c:Vision_OptyProductsPartialMatchEvent" action="{!c.handleCheckboxEvent}"/>
    <aura:attribute name="autoCalcUnits" type="boolean" default="true"/>
    <aura:attribute name="showDiscrepancyMsg" type="boolean" default="false" />
    <aura:attribute name="discrepancyMessage" type="string"/>
    <aura:attribute name="selMatchPrdId" type="string" />
    <aura:attribute name="hideUploadedItems" type="boolean" default="false"/>
    <aura:attribute name="hideUploadedItemsUnMatch" type="boolean" default="false"/>
    <aura:attribute name="hideUploadedDiscrepancy" type="boolean" default="false"/>
    <aura:attribute name="hideDrlItems" type="boolean" default="false"/>
    <aura:attribute name="isViewAllMatched" type="boolean" default="true"/>
    <aura:attribute name="isViewPartialMatched" type="boolean" default="true"/>
    <aura:attribute name="mergeObj" type="object"/>
    <aura:attribute name="showMergePopup" type="boolean" default="false"/>
    <aura:attribute name="isMergeComplete" type="boolean" default="false"/>
    <aura:attribute name="accId" type="string"/>
    
    <aura:if isTrue="{!v.showMergePopup}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container modalPopupClass">
                <div class="modal-header slds-modal__header" style="">
                    <h2 class="slds-modal__title slds-hyphenate"
                        style="color:#675e5e">
                        <p style="font-size:21px;"><b>Merge Proposed Units for - {!v.mergeObj.prodName} - Product</b></p> 
                    </h2>
                </div>
                <div class="slds-p-around--medium" style="background-color:white;padding-bottom: 10px;">
                    <c:MergeProposedUnitsCmp dupList="{!v.mergeObj.selectedDupList}" isMergeComplete="{!v.isMergeComplete}" 
                                             refreshScreen ="{!v.refreshScreen}" showMergePopup="{!v.showMergePopup}"
                                             AccTemplate="{!v.AccTemplate}" isShowInDirectUnits="{!v.isShowInDirectUnits}" isShowDirectUnits="{!v.isShowDirectUnits}"/> 
                </div>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    
    <!-- upto here -->
    <aura:if isTrue="{!v.isSpinnerLoad}">
        <center>
            <section role="dialog" aura:id="modalBlockUI" tabindex="-1" aria-labelledby="modal-heading-01"
                     aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open modalShadowRemove">  
                <div class="slds-modal__container" style="background-Color: transparent; border: 0;">  
                    <div class="slds-modal__content slds-p-around_medium modalShadowRemove" style="background-Color: White; border: 0;"> 
                        <div>
                            <img src="{!$Resource.LoadingProgress}" />  
                        </div><br/>
                        <aura:if isTrue="{!v.loadingMessage}">
                            <div style="font-size:18px;color:black;">
                                {!v.loadingMessage}
                            </div>
                            
                        </aura:if>
                    </div>  
                </div>  
            </section>   
            <div class="slds-backdrop slds-backdrop-open" aura:id="customModal-Back"></div>
        </center>
    </aura:if>
    
    
    <div class="slds-grid slds-gutters slds-text-align_center slds-m-around_x-small" style="font-size:16px;">
        <div class="slds-col slds-align_absolute-center slds-size_1-of-5">
            <span><h1>Total Products: {! v.TotalRecords }</h1></span>
        </div>
        <div class="slds-col slds-align_absolute-center slds-text-color_success slds-size_1-of-5">
            <span><h1>Matched Products: {! v.matchedRowsCount }</h1></span>
        </div>
        <div class="slds-col slds-align_absolute-center slds-text-color_error slds-size_1-of-5">
            <span><h1>Unmatched Products: {! v.unmatchedRowsCount }</h1></span>
        </div>
        <div class="slds-col slds-align_absolute-center slds-size_1-of-5">
            <span style="color:#9d56b8;"><h1>Discrepancy Products: {! v.discRowsCount }</h1></span>
        </div>
        <div class="slds-col slds-size_1-of-5" style="cursor:pointer;">
            <span onclick="{!c.handleInfoClick}">
                Detailed Summary&nbsp;&nbsp;<lightning:Icon iconName="utility:info" variant="border-filled"
                                                            iconClass="dark" onclick="{! c.handleInfoClick}" 
                                                            alternativeText="Info" title="Info" size="x-small"/>
            </span>
        </div>
    </div>
    

    <div class="slds-box" style="padding:10px;">
        <div class="slds-grid">
            <div class="slds-col slds-size_3-of-9">
                <div style="margin-top: 10px; padding-left: 10px; max-width: 500px;">
                    <span onkeyup="{!c.onKeyUpHandler}" onkeydown="{!c.onKeyDownHandler}">
                        <lightning:input value="{!v.searchText}" type="search" aura:id="SearchBox"
                                         variant="label-hidden" placeholder="Search by DRL Product Name, Customer's Product Description and NDC"/>
                    </span>
                </div>
            </div>
            <!-- <div class="slds-col slds-size_1-of-6" style="margin-top: 10px;text-align:right;">
                <aura:if isTrue="{!v.selTabId == 'matchPrds'}">
                    <b>View Perfectly Matched Items</b>
                </aura:if>
            </div>
            <div class="slds-col slds-size_1-of-6" style="width:60px;padding-top:10px;">
                <aura:if isTrue="{!v.selTabId == 'matchPrds'}">
                    <lightning:input type="toggle" checked="{!v.isViewAllMatched}"
                                     messageToggleActive="" messageToggleInactive=""
                                     onchange="{!c.changeMatchedViewItems}" value="{!v.isViewAllMatched}"/>
                </aura:if>
            </div>
            <div class="slds-col slds-size_1-of-6" style="margin-top: 10px;text-align:left;">
                <aura:if isTrue="{!v.selTabId == 'matchPrds'}">
                    <b>View All Matched Items</b>
                </aura:if>
            </div> -->
            <div class="slds-col slds-size_2-of-9"/>
            <div class="slds-col slds-size_1-of-9" style="padding-top:15px;">
                <aura:if isTrue="{!v.selTabId == 'matchPrds'}">
                    <lightning:input type="toggle" checked="{!v.isViewAllMatched}"
                                     messageToggleActive="" messageToggleInactive=""
                                     onchange="{!c.changeMatchedViewItems}" value="{!v.isViewAllMatched}"
                                     label="View Perfectly Matched Items"/>
                </aura:if>
            </div>
            <div class="slds-col slds-size_1-of-9" style="padding-top:15px;">
                <aura:if isTrue="{!v.selTabId == 'matchPrds'}">
                    <lightning:input type="toggle" checked="{!v.isViewPartialMatched}"
                                     messageToggleActive="" messageToggleInactive=""
                                     value="{!v.isViewPartialMatched}"
                                     label="View Partially Matched Items"/>
                </aura:if>
            </div>
            <div class="slds-col slds-size_2-of-9" style="text-align:right;padding-top:15px;padding-right:15px;">
                <h2>To View the changes made, please Refresh.
                    <lightning:icon iconName="utility:sync" alternativeText="Refresh" 
                                    size="x-small" class="showGreen" 
                                    style="cursor:pointer;" onclick="{!c.doInit}"/>
                </h2>
            </div>
        </div>
        <table>
            <td style="{!v.selTabId == 'matchPrds' ? 'border-bottom: 3px solid #4194f9;font-weight:bold;font-size:14px;cursor:pointer;text-align:center;padding:10px 0;' : 'cursor:pointer;text-align:center;'}"
                onclick="{!c.matchedProductsTabSelected}" width="150px;">
                <p style="">Matched Products</p>
            </td>
            <td style="{!v.selTabId == 'unmatchPrds' ? 'border-bottom: 3px solid #4194f9;font-weight:bold;font-size:14px;cursor:pointer;text-align:center;padding:10px 0;' : 'cursor:pointer;text-align:center;'}"
                onclick="{!c.unmatchedProductsTabSelected}" width="150px;">
                <p style="">Unmatched Products</p>
            </td>
            <td style="{!v.selTabId == 'discPrds' ? 'border-bottom: 3px solid #4194f9;font-weight:bold;font-size:14px;cursor:pointer;text-align:center;padding:10px 0;' : 'cursor:pointer;text-align:center;'}"
                onclick="{!c.discrepancyProductsTabSelected}" width="150px;">
                <p style="">Discrepancy Products</p>
            </td>
            <td style="text-align:right;padding-right:12px;">
                <aura:if isTrue="{!not(empty(v.prodOptyIdList ))}">
                    <lightning:button class="slds-m-top_small" variant="brand" label="Move To Match" 
                                      onclick="{!c.moveToMatchHandler}" disabled=" "/>
                </aura:if>
                <aura:if isTrue="{!v.selTabId == 'matchPrds'}">
                    
                </aura:if>
                <aura:if isTrue="{!v.selTabId == 'unmatchPrds'}">
                    <!-- <lightning:button class="slds-m-top_small" variant="brand" label="Re-match Products" 
                                      onclick="{!c.saveChangeRecords}" disabled="{!v.sentToPanorama}"/>
                    <lightning:button class="slds-m-top_small" variant="brand" 
                                      label="Save" disabled="false" onclick="{!c.saveOptyLineItems}"/> -->
                </aura:if>
                <aura:if isTrue="{!v.selTabId == 'discPrds'}">
                    <!-- <lightning:button class="slds-m-top_small" variant="brand" label="Save" 
                                      disabled="false" onclick="{!c.saveOptyLineItems}"/> -->
                </aura:if>
            </td>
        </table>
        
        <!--<lightning:tabset selectedTabId="{!v.selTabId}" onselect="{!c.tabSelected}">
            <lightning:tab label="Matched Products" id="matchPrds">-->
        <aura:if isTrue="{!v.selTabId == 'matchPrds'}">
            <aura:if isTrue="{!v.matchdata.length == 0}">
                <lightning:card >
                    <center>
                        <div class="slds-notify slds-notify_alert slds-theme_alert-texture slds-theme_info slds-m-left_xx-large slds-m-right_xx-large" role="alert" style="width:50%;">
                            <span class="slds-icon_container slds-m-right_x-small" title="No data found.">
                                <lightning:icon iconName="utility:info" alternativeText="info" size="x-small" variant="inverse"/>
                            </span>
                            <h2>No records found.</h2>
                        </div>
                    </center>
                </lightning:card>
                <aura:set attribute="else">
                    <!-- <div class="pagination2">
                        <div class="navlink nextlink">
                            <lightning:button class="slds-m-top_small" variant="brand" label="Select Contracts" 
                                              onclick="{!c.moveToContracts}" disabled="{!v.stageName != 'Opportunity in progress'}"/>
                        </div>
                    </div> -->
                    <div style="padding:10px;padding-bottom:0px;">
                        <div id="content" class="slds-scrollable matchedTable" style="height:400px;">
                            <table class="customtable slds-table slds-table_cell-buffer slds-table_bordered slds-table_col-bordered" role="grid">
                                <thead>
                                    <tr class="slds-text-title--caps">
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th class="prodFamSticky" style="background-color: #a3c2c2;text-align:right !important;">
                                            <aura:if isTrue="{!v.hideUploadedItems}">
                                                <lightning:icon class="showBlack" size="x-small" iconName="utility:chevronright" 
                                                                title="Show Uploaded Columns" onclick="{!c.showHideUploadedItems}" style="cursor:pointer;"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon class="showBlack" size="x-small" iconName="utility:chevronleft" 
                                                                    title="Hide Uploaded Columns" onclick="{!c.showHideUploadedItems}" style="cursor:pointer;"/>
                                                </aura:set>
                                            </aura:if>
                                        </th>
                                        <aura:if isTrue="{!!v.hideUploadedItems}">
                                            <th style="text-align:center;background-color:#D2B4DE;" colSpan="8">
                                              Customer Uploaded Data
                                            </th>
                                        </aura:if>
                                        <th style="text-align:right !important;">
                                            <aura:if isTrue="{!v.hideDrlItems}">
                                                <lightning:icon class="showBlack" size="x-small" iconName="utility:chevronright" 
                                                                title="Show DRL Columns" onclick="{!c.showHideDrlDiscrepancy}" style="cursor:pointer;"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon class="showBlack" size="x-small" iconName="utility:chevronleft" 
                                                                    title="Hide DRL Columns" onclick="{!c.showHideDrlDiscrepancy}" style="cursor:pointer;"/>
                                                </aura:set>
                                            </aura:if>
                                        </th>
                                        <aura:if isTrue="{!!v.hideDrlItems}">
                                            <th style="text-align:center;background-color:#659153;color:white;" colSpan="9">
                                                DRL Data
                                            </th>
                                        </aura:if>
                                        <th colSpan="15"/>
                                    </tr>
                                    <tr class="slds-text-title--caps">
                                        <th scope="col" style="padding: 4px;">
                                            <div class="slds-truncate" title="GCN">C</div>
                                        </th>
                                        <th scope="col" style="padding: 4px;">
                                            <div class="slds-truncate" title="GSN">S</div>
                                        </th>
                                        <th scope="col" style="padding: 4px;">
                                            <div class="slds-truncate" title="GPI">P</div>
                                        </th>
                                        <th scope="col" style="padding: 4px;">
                                            <div class="slds-truncate" title="NDC">N</div>
                                        </th>
                                        <th scope="col" style="padding: 4px;">
                                            <div class="slds-truncate" title="SKU">I</div>
                                        </th>
                                        <th scope="col" style="padding: 4px;">
                                            <div class="slds-truncate" title="BID ON HIGHEST PACK SIZE">H</div>
                                        </th>
                                        <th scope="col" style="padding: 4px;">
                                            <div class="slds-truncate" title="BID ON ALTERNATIVE PACK SIZE">A</div>
                                        </th>
                                      <th scope="col" style="background-color: #669999;color: white">
                                            <div class="slds-truncate" title="Product Name"> DRL Product Name 
                                                <span onclick="{!c.sortByName}" style="cursor: pointer;">  
                                                    <aura:if isTrue="{!v.isAsc}">
                                                         <lightning:icon iconName="utility:arrowup"  size="xx-small" alternativeText="" class="white"/>
                                                        <aura:set attribute="else">
                                                           <lightning:icon iconName="utility:arrowdown"  size="xx-small" alternativeText="" class="white"/>
                                                        </aura:set>
                                                    </aura:if>
                                                </span>
                                            </div>
                                        </th>
                                        <aura:if isTrue="{!!v.hideUploadedItems}">
                                            
                                            <th scope="col" style="background-color: #6c3483;color: white">
                                                <div class="slds-truncate slds-cell-wrap" title="NDC">NDC</div>
                                            </th>
                                            <th scope="col" style="background-color: #6c3483;color: white">
                                                <div class="slds-truncate slds-cell-wrap" title="SKU">SKU</div>
                                            </th>
                                            <th scope="col" style="background-color: #6c3483;color: white">
                                                <div class="slds-truncate slds-cell-wrap" title="GCN">GCN</div>
                                            </th>
                                            <th scope="col" style="background-color: #6c3483;color: white">
                                                <div class="slds-truncate slds-cell-wrap" title="GPI">GPI</div>
                                            </th>
                                            <th scope="col" style="background-color: #6c3483;color: white">
                                                <div class="slds-truncate slds-cell-wrap" title="GSN">GSN</div>
                                            </th>
                                            <th scope="col" style="background-color: #6c3483;color: white">
                                                <div class="slds-truncate slds-cell-wrap" title="Pack Size">PACK SIZE</div>
                                            </th>
                                            <th scope="col" style="background-color: #6c3483;color: white">
                                                <div class="slds-truncate slds-cell-wrap" title="Pack Size">STRENGTH</div>
                                            </th>
                                            
                                            <th scope="col" style="background-color: #6c3483;color: white"><!--onclick="{!c.sortByDescription}" -->
                                                <div class="slds-truncate" title="Product Description">PRODUCT DESCRIPTION
                                                    <!--<span>  <aura:if isTrue="{!v.isDescAsc}"><lightning:icon iconName="utility:arrowup"  size="xx-small" alternativeText=""/>
                                                        <aura:set attribute="else">
                                                            <lightning:icon iconName="utility:arrowdown"  size="xx-small" alternativeText=""/>
                                                        </aura:set>
                                                        </aura:if>
                                                    </span>-->
                                                </div>
                                            </th>
                                        </aura:if>
                                        
                                       
                                        <!-- <th scope="col">
                                            <div class="slds-truncate slds-cell-wrap" title="Uploaded Pack Size">UPLOADED PACK SIZE</div>
                                        </th> -->
                                        <aura:if isTrue="{!!v.hideDrlItems}">
