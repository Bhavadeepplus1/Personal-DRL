<aura:component controller="Vision_OpportunityCreator" implements="flexipage:availableForRecordHome,force:hasRecordId" access="global" >
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="draftValues" type="Object" default="[]"/>
    <aura:attribute name="oppData" type="Object"/>
    <aura:attribute name="successVal" type="string"/>
    <aura:attribute name="TotalRecords" type="Integer" default="0"/>
    <aura:attribute name="showProductsloading" type="Boolean" default="false" />
    <aura:attribute name="matchdata" type="List" default="[]"/>
    <aura:attribute name="unmatchdata" type="List" default="[]"/>
