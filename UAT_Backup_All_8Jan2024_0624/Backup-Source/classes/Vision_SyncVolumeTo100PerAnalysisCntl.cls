public class Vision_SyncVolumeTo100PerAnalysisCntl {
    @auraEnabled
    public static string syncVolToAnalysis(String bidId, String accId, boolean isFullLineRfp){
        updateInAnalysisClass(bidId, accId, isFullLineRfp);
        return 'DONE';
    }
    
    @future
    public static void updateInAnalysisClass(String bidId, String accId, boolean isFullLineRfp){
       List<Volume_History__c> syncRecList = new List<Volume_History__c>();
        try{
            List<String> prodFamIds = new List<String>();
            Map<String, Phoenix_Bid_Line_Item__c> prodTotalUnitsMap = new Map<String, Phoenix_Bid_Line_Item__c>();
            Account acc = [SELECT Id, Name, AccountNumber FROM Account WHERE Id =: accId];
            
            String query = 'select Phoenix_Product__r.Product_Family__c,Phoenix_Bid__r.Phoenix_Customer__c, Phoenix_Product__r.Product_Family__r.Name, Phoenix_Bid__r.Phoenix_Customer_Type__c, ' + Phoenix_Util.getsObjectFieds('Phoenix_Bid_Line_Item__c') +',( SELECT '+Phoenix_Util.getsObjectFieds('Phoenix_BidLineItemExtn__c')+' FROM BidLineItemsExtn__r) FROM Phoenix_Bid_Line_Item__c where Phoenix_Bid__c = : bidId';
            
            list < Phoenix_Bid_Line_Item__c > bidLineItemsList = Database.query(query);
            List<String> relList = new List<String>();
            
            List<Id> prodIds = new List<Id>();
            for(Phoenix_Bid_Line_Item__c bidLineObj:bidLineItemsList){
                bidLineObj.Phoenix_Proposed_OS_Units__c = (bidLineObj.Phoenix_Proposed_OS_Units__c !=null ? bidLineObj.Phoenix_Proposed_OS_Units__c : 0 );
                bidLineObj.Phoenix_Proposed_RAD_Units__c = (bidLineObj.Phoenix_Proposed_RAD_Units__c !=null ? bidLineObj.Phoenix_Proposed_RAD_Units__c : 0 );
                bidLineObj.Phoenix_Proposed_WMT_Units__c = (bidLineObj.Phoenix_Proposed_WMT_Units__c !=null ? bidLineObj.Phoenix_Proposed_WMT_Units__c : 0 );
                bidLineObj.Phoenix_Proposed_Smith_Drug_Units__c = (bidLineObj.Phoenix_Proposed_Smith_Drug_Units__c !=null ? bidLineObj.Phoenix_Proposed_Smith_Drug_Units__c : 0 );
                bidLineObj.Phoenix_Proposed_Anda_Units__c = (bidLineObj.Phoenix_Proposed_Anda_Units__c !=null ? bidLineObj.Phoenix_Proposed_Anda_Units__c : 0 );
                bidLineObj.Phoenix_ProposedDirectAholdDelhaizeUnits__c = (bidLineObj.Phoenix_ProposedDirectAholdDelhaizeUnits__c !=null ? bidLineObj.Phoenix_ProposedDirectAholdDelhaizeUnits__c : 0 );
                bidLineObj.Phoenix_ProposedDirectGaintEagleUnits__c = (bidLineObj.Phoenix_ProposedDirectGaintEagleUnits__c !=null ? bidLineObj.Phoenix_ProposedDirectGaintEagleUnits__c : 0 );
                bidLineObj.Phoenix_ProposedIndirectAholdDelhaizeUni__c = (bidLineObj.Phoenix_ProposedIndirectAholdDelhaizeUni__c !=null ? bidLineObj.Phoenix_ProposedIndirectAholdDelhaizeUni__c : 0 );
                bidLineObj.Phoenix_Others_Direct__c = (bidLineObj.Phoenix_Others_Direct__c !=null ? bidLineObj.Phoenix_Others_Direct__c : 0 );
                bidLineObj.Phoenix_Others_Indirect__c = (bidLineObj.Phoenix_Others_Indirect__c !=null ? bidLineObj.Phoenix_Others_Indirect__c : 0 );
                bidLineObj.Phoenix_Direct_ESI__c = (bidLineObj.Phoenix_Direct_ESI__c !=null ? bidLineObj.Phoenix_Direct_ESI__c : 0 );
                bidLineObj.Phoenix_Indirect_ESI__c = (bidLineObj.Phoenix_Indirect_ESI__c !=null ? bidLineObj.Phoenix_Indirect_ESI__c : 0 );
                bidLineObj.Phoenix_Direct_Kroger__c = (bidLineObj.Phoenix_Direct_Kroger__c !=null ? bidLineObj.Phoenix_Direct_Kroger__c :0 );
                bidLineObj.Phoenix_Indirect_Kroger__c = (bidLineObj.Phoenix_Indirect_Kroger__c !=null ? bidLineObj.Phoenix_Indirect_Kroger__c : 0 );
                bidLineObj.Phoenix_Direct_Rx_Outreach__c = (bidLineObj.Phoenix_Direct_Rx_Outreach__c !=null ? bidLineObj.Phoenix_Direct_Rx_Outreach__c : 0 );
                bidLineObj.Phoenix_Indirect_Rx_Outreach__c = (bidLineObj.Phoenix_Indirect_Rx_Outreach__c !=null ? bidLineObj.Phoenix_Indirect_Rx_Outreach__c : 0 );
                bidLineObj.Phoenix_Direct_Supervalu__c = (bidLineObj.Phoenix_Direct_Supervalu__c !=null ? bidLineObj.Phoenix_Direct_Supervalu__c : 0 );
                bidLineObj.Phoenix_Indirect_Supervalu__c = (bidLineObj.Phoenix_Indirect_Supervalu__c !=null ? bidLineObj.Phoenix_Indirect_Supervalu__c : 0 );
                bidLineObj.Phoenix_Direct_Cordant__c = (bidLineObj.Phoenix_Direct_Cordant__c !=null ? bidLineObj.Phoenix_Direct_Cordant__c : 0 );
                bidLineObj.Phoenix_Indirect_Cordant__c = (bidLineObj.Phoenix_Indirect_Cordant__c !=null ? bidLineObj.Phoenix_Indirect_Cordant__c : 0 );
                bidLineObj.Phoenix_Direct_Cigna__c = (bidLineObj.Phoenix_Direct_Cigna__c !=null ? bidLineObj.Phoenix_Direct_Cigna__c : 0 );
                bidLineObj.Phoenix_Indirect_Cigna__c = (bidLineObj.Phoenix_Indirect_Cigna__c !=null ? bidLineObj.Phoenix_Indirect_Cigna__c : 0 );
                bidLineObj.Phoenix_Indirect_Accerodo__c = (bidLineObj.Phoenix_Indirect_Accerodo__c !=null ? bidLineObj.Phoenix_Indirect_Accerodo__c : 0 );
                bidLineObj.Phoenix_Direct_Accerodo__c = (bidLineObj.Phoenix_Direct_Accerodo__c !=null ? bidLineObj.Phoenix_Direct_Accerodo__c :0 );
                bidLineObj.Phoenix_Proposed_Indirect_Selling_Unit__c = bidLineObj.Phoenix_Bid__r.Phoenix_Customer_Type__c == 'RXSS' ? 0 : (bidLineObj.Phoenix_Proposed_Indirect_Selling_Unit__c !=null ? bidLineObj.Phoenix_Proposed_Indirect_Selling_Unit__c : 0 );
                bidLineObj.Phoenix_Proposed_Direct_Selling_Unit__c = bidLineObj.Phoenix_Bid__r.Phoenix_Customer_Type__c == 'RXSS' ? 0 : (bidLineObj.Phoenix_Proposed_Direct_Selling_Unit__c !=null ? bidLineObj.Phoenix_Proposed_Direct_Selling_Unit__c : 0 );
                
                bidLineObj.Vision_Analysis_Volume__c = bidLineObj.Phoenix_Proposed_OS_Units__c+bidLineObj.Phoenix_Proposed_RAD_Units__c+bidLineObj.Phoenix_Proposed_WMT_Units__c+
                    bidLineObj.Phoenix_ProposedDirectAholdDelhaizeUnits__c+bidLineObj.Phoenix_Proposed_Smith_Drug_Units__c+bidLineObj.Phoenix_Proposed_Anda_Units__c+
                    bidLineObj.Phoenix_ProposedDirectGaintEagleUnits__c+bidLineObj.Phoenix_ProposedIndirectAholdDelhaizeUni__c+bidLineObj.Phoenix_Others_Direct__c+
                    bidLineObj.Phoenix_Others_Indirect__c+bidLineObj.Phoenix_Direct_ESI__c+bidLineObj.Phoenix_Indirect_ESI__c+bidLineObj.Phoenix_Direct_Kroger__c+bidLineObj.Phoenix_Indirect_Kroger__c+
                    bidLineObj.Phoenix_Direct_Rx_Outreach__c+bidLineObj.Phoenix_Indirect_Rx_Outreach__c+bidLineObj.Phoenix_Direct_Supervalu__c+bidLineObj.Phoenix_Indirect_Supervalu__c+bidLineObj.Phoenix_Direct_Cordant__c+
                    bidLineObj.Phoenix_Indirect_Cordant__c+bidLineObj.Phoenix_Direct_Cigna__c+bidLineObj.Phoenix_Indirect_Cigna__c+bidLineObj.Phoenix_Indirect_Accerodo__c+
                    bidLineObj.Phoenix_Direct_Accerodo__c+bidLineObj.Phoenix_Proposed_Indirect_Selling_Unit__c+bidLineObj.Phoenix_Proposed_Direct_Selling_Unit__c;
                
                if(!relList.contains(bidLineObj.Phoenix_Product__r.Product_Family__r.Name))
                    relList.add(bidLineObj.Phoenix_Product__r.Product_Family__r.Name);
                prodIds.add(bidLineObj.Phoenix_Product__c);
                prodTotalUnitsMap.put(bidLineObj.Phoenix_Product__c, bidLineObj);
                string famId = bidLineObj.Phoenix_Product__r.Product_Family__c;
                if(!prodFamIds.contains(famId) && famId != null)
                    prodFamIds.add(famId);
                
                Volume_History__c syncrec = new Volume_History__c();
                syncrec.Account__c = bidLineObj.Phoenix_Bid__r.Phoenix_Customer__c;
                syncrec.Bid_Line_Item__c = bidLineObj.Id;
                syncrec.Is_Full_Line__c = isFullLineRfp;
                syncrec.Phoenix_Bid__c = bidId;
                syncrec.Product__c = bidLineObj.Phoenix_Product__c;
                syncrec.Status__c = 'Success';
                				syncrec.Sales_Team_Member__c = userInfo.getUserId();

                syncrec.Total_Proposed_Units__c = bidLineObj.Vision_Analysis_Volume__c;
                syncRecList.add(syncrec);
            }
            
            String childQuery = 'SELECT Vision_Customer__r.Name, Vision_Customer__r.Vision_is_SRx_Account__c,Vision_Product__r.Product_Family__c, Vision_Product__r.Id,Vision_Product__r.Name, Vision_Product__r.Phoenix_NDC_11__c, Vision_Product__r.isActive,Vision_Product__r.Phoenix_Pkg_Size__c, '+
                +' Vision_Customer__r.Phoenix_Account_Cooling_Period__c, '+Phoenix_Util.getsObjectFieds('GCP_SFDC_100pct_Analysis_SKU__c')+' FROM GCP_SFDC_100pct_Analysis_SKU__c'+
                +' WHERE Vision_Customer__c =: accId AND Vision_Product__r.Product_Family__c IN: prodFamIds AND Vision_Product__c != null AND Vision_Product__r.isActive = true '+
                +' AND (Vision_Product__r.Phoenix_Is_Private_Label_OTC__c = false OR (Vision_Product__r.Phoenix_Is_Private_Label_OTC__c = true AND Vision_Product__r.OTC_Customer__c = : accId AND Vision_Product__r.Phoenix_Is_Control_Label__c = false))'+
                +' AND Vision_Product__r.Phoenix_Is_Exclude_Bid__c = false ORDER BY Vision_Product__r.Name';
            
            List<GCP_SFDC_100pct_Analysis_SKU__c> gcpNdcList = Database.query(childQuery);
            map<string, GCP_SFDC_100pct_Analysis_SKU__c> gcpNdcMap = new map<string, GCP_SFDC_100pct_Analysis_SKU__c>();
            for(GCP_SFDC_100pct_Analysis_SKU__c obj : gcpNdcList){
                if(!prodIds.contains(obj.Vision_Product__c))
                    prodIds.add(obj.Vision_Product__c);
                gcpNdcMap.put(obj.Vision_Product__c,obj);
            }
            
            query = 'SELECT Vision_Product__r.Product_Family__c, Vision_Product__r.Phoenix_Pkg_Size__c, '+Phoenix_Util.getsObjectFieds('GCP_100PCT_Analysis_SKU__c')+' FROM GCP_100PCT_Analysis_SKU__c'+
                +' WHERE Vision_Product__c IN: prodIds AND (Vision_Product__r.Phoenix_Is_Private_Label_OTC__c = false OR (Vision_Product__r.Phoenix_Is_Private_Label_OTC__c = true AND Vision_Product__r.OTC_Customer__c = : accId AND Vision_Product__r.Phoenix_Is_Control_Label__c = false))'+
                +' AND Vision_Product__r.Phoenix_Is_Exclude_Bid__c = false  AND Vision_Product__r.isActive = true';
            List<GCP_100PCT_Analysis_SKU__c> uniqList = Database.query(query);
            List<GCP_SFDC_100pct_Analysis_SKU__c> newSkuList = new List<GCP_SFDC_100pct_Analysis_SKU__c>();
            for(GCP_100PCT_Analysis_SKU__c uniObj : uniqList){
                String prodId = uniObj.Vision_Product__c;
                //if(!prodIds.contains(prodId))
                //    prodIds.add(prodId);
                GCP_SFDC_100pct_Analysis_SKU__c newObj = new GCP_SFDC_100pct_Analysis_SKU__c();
                if(gcpNdcMap.containsKey(prodId))
                    newObj = gcpNdcMap.get(prodId);
                newObj.Vision_Customer__c = accId;
                newObj.Vision_Update_Date__c = uniObj.Vision_Updated_date__c;
                newObj.Vision_Product__c = prodId;
                newObj.Vision_Drl_Act_Price__c = uniObj.DRL_Act_Price__c;
                newObj.Vision_Drl_Act_Sales__c = uniObj.DRL_Act_Sales__c;
                newObj.Vision_DRL_Act_Volume_Eu__c = uniObj.drl_act_volume_eu__c;
                newObj.Vision_DRL_Act_Tpt__c = uniObj.DRL_Act_Tpt__c;
                newObj.GCP_Product_Family__c = uniObj.GCP_Product_Family__c;
                newObj.Vision_Drl_Act_Tpt_Pct__c = uniObj.DRL_Act_Tpt_Pct__c;
                newObj.Product_Family__c = uniObj.Vision_Product__r.Product_Family__c != null ? uniObj.Vision_Product__r.Product_Family__c : null;
                if(prodTotalUnitsMap.containsKey(prodId)){
                    Phoenix_Bid_Line_Item__c prd = prodTotalUnitsMap.get(prodId);
                    Integer totalUnits = Integer.valueOF(prd.Vision_Analysis_Volume__c);
                    if(acc.AccountNumber == '153363' && totalUnits > 0){
                        newObj.Vision_McKesson_Vol__c = prd.Phoenix_Proposed_OS_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                        newObj.Vision_Rite_Aid_Vol__c = prd.Phoenix_Proposed_RAD_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                        newObj.Vision_WalMart_Vol__c = prd.Phoenix_Proposed_WMT_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                        newObj.Vision_McKesson_Vol_Share__c = ((newObj.Vision_McKesson_Vol__c/totalUnits)*100).setScale(2);
                        newObj.Vision_Rite_Aid_Vol_Share__c = ((newObj.Vision_Rite_Aid_Vol__c/totalUnits)*100).setScale(2);
                        newObj.Vision_WalMart_Vol_Share__c = ((newObj.Vision_WalMart_Vol__c/totalUnits)*100).setScale(2);
                        newObj.Vision_NorthStar_Vol_Share__c = (newObj.Vision_NorthStar_Vol__c!= null ? ((newObj.Vision_NorthStar_Vol__c/totalUnits)*100).setScale(2) : 0);
                        newObj.Vision_Estimate_Account_Total_Vol_EU__c = totalUnits + (newObj.Vision_NorthStar_Vol__c!= null ? newObj.Vision_NorthStar_Vol__c : 0);
                    }
                    else{
                        newObj.Vision_Estimate_Account_Total_Vol_EU__c = totalUnits;
                        if(acc.Name == 'Red Oak Sourcing' && totalUnits > 0){
                            newObj.Vision_CVS_Share_Vol__c = (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_DirSellingUnits__c != null ? prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_DirSellingUnits__c : (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_IndirSellingUnits__c !=null ? prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_IndirSellingUnits__c : 0));//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                            newObj.Vision_Contract_Type__c = (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_DirSellingUnits__c != null ? 'Direct' : (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_IndirSellingUnits__c !=null ? 'Indirect' : 'Direct'));
                            newObj.Vision_Cardinal_Share_Vol__c = prd.BidLineItemsExtn__r[0].Phoenix_Proposed_Cardinal_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                            newObj.Vision_Major_Share_Vol__c = prd.BidLineItemsExtn__r[0].Phoenix_Proposed_Major_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                            newObj.Vision_CVS_VolShare_Percent__c = ((newObj.Vision_CVS_Share_Vol__c/totalUnits)*100).setScale(2);
                            newObj.Vision_Cardinal_VolShare_Percent__c = ((newObj.Vision_Cardinal_Share_Vol__c/totalUnits)*100).setScale(2);
                            newObj.Vision_Major_VolShare_Percent__c = ((newObj.Vision_Major_Share_Vol__c/totalUnits)*100).setScale(2);
                        }
                    }
                    newObj.Vision_Estimate_Vol_Method__c = 'Full Line Volume';
                }
                newObj.Vision_Modified_Date__c = system.today();
                //else
                //     newObj.Vision_Estimate_Account_Total_Vol_EU__c = 0;
                newSkuList.add(newObj);
            }
            //upsert newSkuList;
            //newSkuList = new List<GCP_SFDC_100pct_Analysis_SKU__c>();
            List<Product2> prodList = [SELECT Id, Name, Product_Family__c, Product_Family__r.Name, Phoenix_Pkg_Size__c FROM Product2 WHERE Product_Family__c IN: prodFamIds AND Id NOT IN: prodIds
                                       AND (Phoenix_Is_Private_Label_OTC__c = false OR (Phoenix_Is_Private_Label_OTC__c = true AND OTC_Customer__c = : accId AND Phoenix_Is_Control_Label__c = false))
                                       AND isActive = true AND Phoenix_Is_Exclude_Bid__c = false];
            for(Product2 prodObj : prodList){
                GCP_SFDC_100pct_Analysis_SKU__c newObj = new GCP_SFDC_100pct_Analysis_SKU__c();
                if(gcpNdcMap.containsKey(prodObj.Id))
                    newObj = gcpNdcMap.get(prodObj.Id);
                newObj.Vision_Customer__c = accId;
                newObj.Vision_Product__c = prodObj.Id;
                newObj.GCP_Product_Family__c = prodObj.Product_Family__r.Name;
                newObj.Product_Family__c = prodObj.Product_Family__c;
                if(prodTotalUnitsMap.containsKey(newObj.Vision_Product__c)){
                    Phoenix_Bid_Line_Item__c prd = prodTotalUnitsMap.get(newObj.Vision_Product__c);
                    Integer totalUnits = Integer.valueOF(prd.Vision_Analysis_Volume__c);
                    if(acc.AccountNumber == '153363' && totalUnits > 0){
                        newObj.Vision_McKesson_Vol__c = prd.Phoenix_Proposed_OS_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                        newObj.Vision_Rite_Aid_Vol__c = prd.Phoenix_Proposed_RAD_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                        newObj.Vision_WalMart_Vol__c = prd.Phoenix_Proposed_WMT_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                        newObj.Vision_McKesson_Vol_Share__c = ((newObj.Vision_McKesson_Vol__c/totalUnits)*100).setScale(2);
                        newObj.Vision_Rite_Aid_Vol_Share__c = ((newObj.Vision_Rite_Aid_Vol__c/totalUnits)*100).setScale(2);
                        newObj.Vision_WalMart_Vol_Share__c = ((newObj.Vision_WalMart_Vol__c/totalUnits)*100).setScale(2);
                        newObj.Vision_NorthStar_Vol_Share__c = (newObj.Vision_NorthStar_Vol__c!= null ? ((newObj.Vision_NorthStar_Vol__c/totalUnits)*100).setScale(2) : 0);
                        newObj.Vision_Estimate_Account_Total_Vol_EU__c = totalUnits + newObj.Vision_NorthStar_Vol__c;
                    }
                    else{
                        newObj.Vision_Estimate_Account_Total_Vol_EU__c = totalUnits;
                        if(acc.Name == 'Red Oak Sourcing' && totalUnits > 0){
                            newObj.Vision_CVS_Share_Vol__c = (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_DirSellingUnits__c != null ? prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_DirSellingUnits__c : (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_IndirSellingUnits__c !=null ? prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_IndirSellingUnits__c : 0));//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                            newObj.Vision_Contract_Type__c = (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_DirSellingUnits__c != null ? 'Direct' : (prd.BidLineItemsExtn__r[0].Phoenix_Proposed_CVS_IndirSellingUnits__c !=null ? 'Indirect' : 'Direct'));
                            newObj.Vision_Cardinal_Share_Vol__c = prd.BidLineItemsExtn__r[0].Phoenix_Proposed_Cardinal_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                            newObj.Vision_Major_Share_Vol__c = prd.BidLineItemsExtn__r[0].Phoenix_Proposed_Major_Units__c;//*obj.Vision_Product__r.Phoenix_Pkg_Size__c;
                            newObj.Vision_CVS_VolShare_Percent__c = ((newObj.Vision_CVS_Share_Vol__c/totalUnits)*100).setScale(2);
                            newObj.Vision_Cardinal_VolShare_Percent__c = ((newObj.Vision_Cardinal_Share_Vol__c/totalUnits)*100).setScale(2);
                            newObj.Vision_Major_VolShare_Percent__c = ((newObj.Vision_Major_Share_Vol__c/totalUnits)*100).setScale(2);
                        }
                    }
                    newObj.Vision_Estimate_Vol_Method__c = 'Full Line Volume';
                }
                newObj.Vision_Modified_Date__c = system.today();
                //else
                //    newObj.Vision_Estimate_Account_Total_Vol_EU__c = 0;
                newSkuList.add(newObj);
            }
            if(newSkuList.size()>0)
                upsert newSkuList;
            gcpNdcList = Database.query(childQuery);
            Map<String, Integer> prodFamTotalunits = new Map<String, Integer>();
            for(GCP_SFDC_100pct_Analysis_SKU__c prd : gcpNdcList){
                if(prd.Vision_Product__r.Product_Family__c != null){
                    string famId = prd.Vision_Product__r.Product_Family__c;
                    prd.Vision_Estimate_Account_Total_Vol_EU__c = prd.Vision_Estimate_Account_Total_Vol_EU__c != null? prd.Vision_Estimate_Account_Total_Vol_EU__c : 0;
                    prd.Vision_User_Input_Estimate_Vol__c = prd.Vision_User_Input_Estimate_Vol__c != null? prd.Vision_User_Input_Estimate_Vol__c : 0;
                    prd.Vision_Estimate_Vol_Method__c = (prd.Vision_Estimate_Vol_Method__c != null ? prd.Vision_Estimate_Vol_Method__c : 'Full Line Volume');
                    prd.Vision_Product__r.Phoenix_Pkg_Size__c = prd.Vision_Product__r.Phoenix_Pkg_Size__c != null ? prd.Vision_Product__r.Phoenix_Pkg_Size__c : 1;
                    Integer totalVal = Integer.valueOf((prd.Vision_Estimate_Vol_Method__c == 'Full Line Volume' ? prd.Vision_Estimate_Account_Total_Vol_EU__c  
                                                        : prd.Vision_User_Input_Estimate_Vol__c)*prd.Vision_Product__r.Phoenix_Pkg_Size__c);
                    if(prodFamTotalunits.containsKey(famId)){
                        Integer totalUnits = prodFamTotalunits.get(famId);
                        prodFamTotalunits.put(famId, totalUnits+totalVal);
                    }
                    else
                        prodFamTotalunits.put(famId, totalVal);
                }
            }
            
            List<Phoenix_GCP_Opportunity_Tracker__c> gcpList = [SELECT Id, Vision_isRelevance__c, Vision_Inline_RFP_Units__c, Product_Family__c, Product_Family__r.Name, Phoenix_Est_Acct_Vol_Method__c, Vision_Sum_of_Sku_est_vol__c 
                                                                FROM Phoenix_GCP_Opportunity_Tracker__c WHERE Phoenix_Customer__c =: accId AND Product_Family__c != null AND Vision_isRelevance__c = true];
            List<Phoenix_GCP_Opportunity_Tracker__c> updateGcpList = new List<Phoenix_GCP_Opportunity_Tracker__c>();
            for(Phoenix_GCP_Opportunity_Tracker__c gcpObj : gcpList){
                if(!isFullLineRfp){
                    if(!relList.contains(gcpObj.Product_Family__r.Name))
                        relList.add(gcpObj.Product_Family__r.Name);
                }
            }
            gcpList = Phoenix_GCP_OptyTracker_Controller.updateGcpListWithNewRelevance(accId, relList);
            for(Phoenix_GCP_Opportunity_Tracker__c gcpObj : gcpList){
                string famId = gcpObj.Product_Family__c;
                if(prodFamTotalunits.containsKey(famId)){
                    gcpObj.Vision_Inline_RFP_Units__c = prodFamTotalunits.get(famId);
                    gcpObj.Phoenix_Est_Acct_Vol_Method__c = 'Use Proposed Total Selling Units (BRIGHT)';
                    updateGcpList.add(gcpObj);
                }
            }
            update updateGcpList;
            system.debug('syncRecList=='+syncRecList);
            insert syncRecList;
